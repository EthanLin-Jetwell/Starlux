@RestResource(urlMapping='/RestGtApi/*')
global without sharing class APT_RestAPI {
    @HttpGet
    global static ResponseResultModel getBooking() {
        String resultMessage = 'failed';
        Integer httpStatusCode = 403;
        List<Case> resultCases;
		ResponseResultModel result = new ResponseResultModel();
        RestRequest req = RestContext.request;
        String [] urlArray = req.requestURI.split('/');
        String chatbotId = urlArray.get(urlArray.size() - 1);
        System.debug(chatbotId);
        try {
			resultCases = [SELECT Id, Chatbot_ID__c, Chatbot_Name__c, Chatbot_Mail__c, Chatbot_UID__c, Chatbot_BookingRef__c, Chatbot_Language__c, Chatbot_CustomerType__c, Chatbot_CallSupportTime__c, Ticket_EMD_Number__c, Booking_Reference_PNR__c, Passenger_Name__c, Chatbot_OnlineSupport__c FROM Case WHERE Chatbot_ID__c =: chatbotId];
			httpStatusCode = 200;
            resultMessage = 'success';
        } catch (Exception ex) {
            resultMessage = ex.getMessage();
        }
		
        result.resultCode = httpStatusCode;
        result.resultMessage = resultMessage;
        List<Id> data = new List<Id>();
        for(Case c : resultCases) {
            data.add(c.Id);
        }
        result.data = data;
        return result;
    }

    @HttpPost 
    global static ResponseResultModel doPost() {
        switch on RestContext.request.requestURI.substringAfterLast('/') {
            when 'Case' { return createCase(); }
            when 'Account' { return createAccount(); }
            when 'Special_Case' { return createSpecial_Case(); }
        }
        return NULL;
    }

    global static ResponseResultModel createCase() 
    {
        RestRequest request = RestContext.request;
        RestResponse response = RestContext.response;  
        List<ApexCase> requestBodyWrapper = (List<ApexCase>)System.JSON.deserialize(request.requestBody.toString(), List<ApexCase>.class);
        System.debug(requestBodyWrapper);
        Integer resultCode = -1;
        //String resultMessage = System.Label.Create_Booking_Fail;
        RestContext.response.statusCode = 403;
        Integer httpStatusCode = 403;
        String resultMessage = 'failed';
        List<Id> data = new List<Id>();
        
        Datetime timeNow = System.now();
        DateTime targetDateTime = DateTime.newInstance(timeNow.date(), Time.newInstance(17, 30, 0, 0));
        
        Temp_Case__c tempCase = new Temp_Case__c();
        for(ApexCase req : requestBodyWrapper) {
            tempCase.Chatbot_ID__c =  req.Chatbot_ID;
            tempCase.Chatbot_Name__c = req.Chatbot_Name;
            tempCase.Chatbot_Mail__c = req.Chatbot_Mail;
            tempCase.Chatbot_UID__c = req.Chatbot_UID;
            tempCase.Chatbot_BookingRef__c = req.Chatbot_BookingRef;
            tempCase.Chatbot_Language__c = req.Chatbot_Language;
            tempCase.Chatbot_CustomerType__c = req.Chatbot_CustomerType;
            tempCase.Chatbot_CallSupportTime__c = req.Chatbot_CallSupportTime;
            tempCase.Ticket_EMD_Number__c = req.Ticket_EMD_Number;
            tempCase.Booking_Reference_PNR__c = req.Booking_Reference_PNR;
            tempCase.Passenger_Name__c = req.Passenger_Name;
            tempCase.Chatbot_OnlineSupport__c = (req.Chatbot_OnlineSupport == '1')? true : false;
            tempCase.Has_Been_Assigned__c = false;
            //pendingTempCases.add(tempCase);
        }
        
        String Language = tempCase.Chatbot_Language__c;
        
        List<Work_Hour__c> workHours = [SELECT Start_Time__c, End_Time__c FROM Work_Hour__c WHERE Language__c = : Language ORDER BY LastModifiedDate];
        Work_Hour__c workHour;
        if(workHours.size() != 0) workHour = workHours[0];
        List<Holiday__c> holidays = [SELECT Date__c FROM Holiday__c WHERE Date__c = : timeNow.date() AND Language__c = : Language];
        //workHours == NULL || timeNow.time() < workHour.Start_Time__c || timeNow.time() > workHour.End_Time__c || holidays.size() != 0 || timeNow.format('u') == '6' || timeNow.format('u') == '7'
        if(workHours == NULL || timeNow.time() < workHour.Start_Time__c || timeNow.time() > workHour.End_Time__c || holidays.size() != 0 || timeNow.format('u') == '6' || timeNow.format('u') == '7') {
            RestContext.response.statusCode = 503;
            httpStatusCode = 503;
            resultMessage = 'No available agent, Our office is currently closed';
        }
        else {
            //Query all chatbot queues and map agents to each queue
            Map<Id, String> queueMap = new Map<Id, String>();
            Map<String, Id> queueIdMap = new Map<String, Id>();
            Map<Id, List<String>> agentMap = new Map<Id, List<String>>();
            List<Group> queue = [SELECT Id, Name FROM Group WHERE Type = 'Queue' AND Name LIKE '%Chat%'];
            List<Id> queueIds = new List<Id>();
            System.debug(queue);
            for(Group g : queue) {
                queueIds.add(g.Id);
                queueMap.put(g.Id, g.Name);
                if(g.Name.contains('(EN)')) {
                    queueIdMap.put('EN', g.Id);
                } else if(g.Name.contains('(JP)')) {
                    queueIdMap.put('JP', g.Id);
                } else if(g.Name.contains('(TW)')) {
                    queueIdMap.put('TW', g.Id);
                } 
                //for First Class routing
                else if(g.Name.contains('(EN-First)')) {
                    queueIdMap.put('EN-First', g.Id);
                } else if(g.Name.contains('(JP-First)')) {
                    queueIdMap.put('JP-First', g.Id);
                } else if(g.Name.contains('(TW-First)')) {
                    queueIdMap.put('TW-First', g.Id);
                }
            }
            
            List<GroupMember> agents = [SELECT Id, UserOrGroupId, GroupId FROM GroupMember WHERE GroupId IN : queueIds];
            Set<Id> userIds = new Set<Id>();
            //System.debug(agents);
            for(GroupMember gm : agents) {
                userIds.add(gm.UserOrGroupId);
            }
            
            Id servicePresenceStatusId = [SELECT Id FROM ServicePresenceStatus WHERE DeveloperName = 'Available' LIMIT 1][0].Id;
            List<UserServicePresence> userPresence = [SELECT Id, UserId FROM UserServicePresence WHERE IsCurrentState = true AND ServicePresenceStatusId = :servicePresenceStatusId AND UserId IN : userIds];
            List<Id> availableUser = new List<Id>();
            System.debug(userPresence);
            for(UserServicePresence USP : userPresence) {
                availableUser.add(USP.UserId);
            }
            
            for(GroupMember gm : agents) {
                if(availableUser.contains(gm.UserOrGroupId)) {
                    //for First Class routing
                    if(queueMap.get(gm.GroupId).contains('EN-First')) {
                        if(!agentMap.keySet().contains(gm.UserOrGroupId)) {
                            agentMap.put(gm.UserOrGroupId, new List<String>());
                        }
                        agentMap.get(gm.UserOrGroupId).add('EN-First');
                    } else if(queueMap.get(gm.GroupId).contains('JP-First')) {
                        if(!agentMap.keySet().contains(gm.UserOrGroupId)) {
                            agentMap.put(gm.UserOrGroupId, new List<String>());
                        }
                        agentMap.get(gm.UserOrGroupId).add('JP-First');
                    } else if(queueMap.get(gm.GroupId).contains('TW-First')) {
                        if(!agentMap.keySet().contains(gm.UserOrGroupId)) {
                            agentMap.put(gm.UserOrGroupId, new List<String>());
                        }
                        agentMap.get(gm.UserOrGroupId).add('TW-First');
                    }
                    
                    else if(queueMap.get(gm.GroupId).contains('EN')) {
                        if(!agentMap.keySet().contains(gm.UserOrGroupId)) {
                            agentMap.put(gm.UserOrGroupId, new List<String>());
                        }
                        agentMap.get(gm.UserOrGroupId).add('EN');
                    } else if(queueMap.get(gm.GroupId).contains('JP')) {
                        if(!agentMap.keySet().contains(gm.UserOrGroupId)) {
                            agentMap.put(gm.UserOrGroupId, new List<String>());
                        }
                        agentMap.get(gm.UserOrGroupId).add('JP');
                    } else if(queueMap.get(gm.GroupId).contains('TW')) {
                        if(!agentMap.keySet().contains(gm.UserOrGroupId)) {
                            agentMap.put(gm.UserOrGroupId, new List<String>());
                        }
                        agentMap.get(gm.UserOrGroupId).add('TW');
                    } 
                }
            }
            
            List<Case> pendingCases = [SELECT Chatbot_Language__c, Chatbot_CustomerType__c FROM Case WHERE Chatbot_UID__c != NULL AND Status != 'Closed' AND CreatedDate = TODAY];
            List<Temp_Case__c> pendingTempCases = [SELECT Chatbot_ID__c, Chatbot_Name__c, Chatbot_Mail__c, Chatbot_UID__c, Chatbot_BookingRef__c, Chatbot_Language__c, Chatbot_CustomerType__c, Chatbot_CallSupportTime__c, Ticket_EMD_Number__c, Booking_Reference_PNR__c, Passenger_Name__c, Chatbot_OnlineSupport__c, Has_Been_Assigned__c FROM Temp_Case__c WHERE Has_Been_Assigned__c = false AND Terminated__c = false AND CreatedDate = TODAY ORDER BY CreatedDate ASC];
            
            List<Temp_Case__c> updateTempCases = new List<Temp_Case__c>();
            //List<Case> upsertCaseList = new List<Case>();
            //List<Temp_Case__c> upsertTempCaseList = new List<Temp_Case__c>();
            try {
                System.debug('before: ' + agentMap);
                //Remove Agents for any number of pending Cases
                for(Case pendingCase : pendingCases) {
                    //for First Class routing
                    if(pendingCase.Chatbot_CustomerType__c == 'VIP') {
                        for(Id availableAgentId : agentMap.keySet()) {
                            if(pendingCase.Chatbot_Language__c == 'English' && agentMap.get(availableAgentId).contains('EN-First')) {
                                agentMap.remove(availableAgentId);
                                break;
                            } else if(pendingCase.Chatbot_Language__c == 'Japanese' && agentMap.get(availableAgentId).contains('JP-First')) {
                                agentMap.remove(availableAgentId);
                                break;
                            } else if(pendingCase.Chatbot_Language__c == 'Mandarin' && agentMap.get(availableAgentId).contains('TW-First')) {
                                agentMap.remove(availableAgentId);
                                break;
                            }
                        }
                    } else {
                        for(Id availableAgentId : agentMap.keySet()) {
                            if(pendingCase.Chatbot_Language__c == 'English' && agentMap.get(availableAgentId).contains('EN')) {
                                agentMap.remove(availableAgentId);
                                break;
                            } else if(pendingCase.Chatbot_Language__c == 'Japanese' && agentMap.get(availableAgentId).contains('JP')) {
                                agentMap.remove(availableAgentId);
                                break;
                            } else if(pendingCase.Chatbot_Language__c == 'Mandarin' && agentMap.get(availableAgentId).contains('TW')) {
                                agentMap.remove(availableAgentId);
                                break;
                            }
                        }
                    }
                }
                //Remove Agents for any number of pending Temp Cases
                for(Temp_Case__c pendingTempCase : pendingTempCases) {
                    //for First Class routing
                    if(pendingTempCase.Chatbot_CustomerType__c == 'VIP') {
                        for(Id availableAgentId : agentMap.keySet()) {
                            if(pendingTempCase.Chatbot_Language__c == 'English' && agentMap.get(availableAgentId).contains('EN-First')) {
                                insertCase(pendingTempCase, queueIdMap.get('EN-First'));
                                pendingTempCase.Has_Been_Assigned__c = true;
                                updateTempCases.add(pendingTempCase);
                                agentMap.remove(availableAgentId);
                                break;
                            } else if(pendingTempCase.Chatbot_Language__c == 'Japanese' && agentMap.get(availableAgentId).contains('JP-First')) {
                                insertCase(pendingTempCase, queueIdMap.get('JP-First'));
                                pendingTempCase.Has_Been_Assigned__c = true;
                                updateTempCases.add(pendingTempCase);
                                agentMap.remove(availableAgentId);
                                break;
                            } else if(pendingTempCase.Chatbot_Language__c == 'Mandarin' && agentMap.get(availableAgentId).contains('TW-First')) {
                                insertCase(pendingTempCase, queueIdMap.get('TW-First'));
                                pendingTempCase.Has_Been_Assigned__c = true;
                                updateTempCases.add(pendingTempCase);
                                agentMap.remove(availableAgentId);
                                break;
                            }
                        }
                    } else {
                        for(Id availableAgentId : agentMap.keySet()) {
                            if(pendingTempCase.Chatbot_Language__c == 'English' && agentMap.get(availableAgentId).contains('EN')) {
                                insertCase(pendingTempCase, queueIdMap.get('EN'));
                                pendingTempCase.Has_Been_Assigned__c = true;
                                updateTempCases.add(pendingTempCase);
                                agentMap.remove(availableAgentId);
                                break;
                            } else if(pendingTempCase.Chatbot_Language__c == 'Japanese' && agentMap.get(availableAgentId).contains('JP')) {
                                insertCase(pendingTempCase, queueIdMap.get('JP'));
                                pendingTempCase.Has_Been_Assigned__c = true;
                                updateTempCases.add(pendingTempCase);
                                agentMap.remove(availableAgentId);
                                break;
                            } else if(pendingTempCase.Chatbot_Language__c == 'Mandarin' && agentMap.get(availableAgentId).contains('TW')) {
                                insertCase(pendingTempCase, queueIdMap.get('TW'));
                                pendingTempCase.Has_Been_Assigned__c = true;
                                updateTempCases.add(pendingTempCase);
                                agentMap.remove(availableAgentId);
                                break;
                            }
                        }
                    }
                }
                
                System.debug('after: ' + agentMap);
                
                Boolean isNewCase = false;
                Id ownerId;
                
                for(Id availableAgentId : agentMap.keySet()) {
                    //for First Class routing
                    if(tempCase.Chatbot_CustomerType__c == 'VIP') {
                        if(tempCase.Chatbot_Language__c == 'English' && agentMap.get(availableAgentId).contains('EN-First')) {
                            ownerId = queueIdMap.get('EN-First');
                            agentMap.remove(availableAgentId);
                            isNewCase = true;
                            break;
                        } else if(tempCase.Chatbot_Language__c == 'Japanese' && agentMap.get(availableAgentId).contains('JP-First')) {
                            ownerId = queueIdMap.get('JP-First');
                            agentMap.remove(availableAgentId);
                            isNewCase = true;
                            break;
                        } else if(tempCase.Chatbot_Language__c == 'Mandarin' && agentMap.get(availableAgentId).contains('TW-First')) {
                            ownerId = queueIdMap.get('TW-First');
                            agentMap.remove(availableAgentId);
                            isNewCase = true;
                            break;
                        }
                    } else {
                        if(tempCase.Chatbot_Language__c == 'English' && agentMap.get(availableAgentId).contains('EN')) {
                            ownerId = queueIdMap.get('EN');
                            agentMap.remove(availableAgentId);
                            isNewCase = true;
                            break;
                        } else if(tempCase.Chatbot_Language__c == 'Japanese' && agentMap.get(availableAgentId).contains('JP')) {
                            ownerId = queueIdMap.get('JP');
                            agentMap.remove(availableAgentId);
                            isNewCase = true;
                            break;
                        } else if(tempCase.Chatbot_Language__c == 'Mandarin' && agentMap.get(availableAgentId).contains('TW')) {
                            System.debug(queueMap.get(queueIdMap.get('TW')));
                            ownerId = queueIdMap.get('TW');
                            agentMap.remove(availableAgentId);
                            isNewCase = true;
                            break;
                        } else if(tempCase.Chatbot_Language__c == NULL) {
                            agentMap.remove(availableAgentId);
                            isNewCase = true;
                            break;
                        }
                    }
                }
                
                if(isNewCase) {
                    System.debug(queueMap.get(ownerId));
                    data.add(insertCase(tempCase, ownerId));
                    RestContext.response.statusCode = 200;
                    httpStatusCode = 200;
                    resultMessage = 'Case created successfully';
                } else {
                    insert tempCase;
                    data.add(tempCase.Id);
                    RestContext.response.statusCode = 201;
                    httpStatusCode = 201;
                    resultMessage = 'No available agent at the time, Temp Case created successfully';
                }
                
                if(updateTempCases.size() > 0) update updateTempCases;
            } catch(exception e) {
                resultMessage = String.valueOf(e);
                System.debug(e.getLineNumber());
            }
        }
        System.debug(new ResponseResultModel(httpStatusCode, resultMessage, data));
        return new ResponseResultModel(httpStatusCode, resultMessage, data);
    }
    
    global static String insertCase(Temp_Case__c pendingTempCase, Id ownerId) 
    {
        Case temp = new Case();
        //assign record type Enquiry and Request
        temp.RecordTypeId = Schema.SObjectType.Case.getRecordTypeInfosByName().get('Enquiry and Request').getRecordTypeId();
        temp.Origin = 'Live Chat';
        temp.Chatbot_ID__c = pendingTempCase.Chatbot_ID__c;
        temp.Chatbot_Name__c = pendingTempCase.Chatbot_Name__c;
        temp.Chatbot_Mail__c = pendingTempCase.Chatbot_Mail__c;
        temp.SuppliedEmail = pendingTempCase.Chatbot_Mail__c;
        temp.Chatbot_UID__c = pendingTempCase.Chatbot_UID__c;
        temp.Chatbot_BookingRef__c = pendingTempCase.Chatbot_BookingRef__c;
        temp.Chatbot_Language__c = pendingTempCase.Chatbot_Language__c;
        temp.Chatbot_CustomerType__c = pendingTempCase.Chatbot_CustomerType__c;
        temp.CTI_Passenger_Type__c = pendingTempCase.Chatbot_CustomerType__c;
        temp.Chatbot_CallSupportTime__c = pendingTempCase.Chatbot_CallSupportTime__c;
        temp.Ticket_EMD_Number__c = pendingTempCase.Ticket_EMD_Number__c;
        temp.Booking_Reference_PNR__c = pendingTempCase.Booking_Reference_PNR__c;
        temp.Passenger_Name__c = pendingTempCase.Passenger_Name__c;
        temp.Chatbot_OnlineSupport__c = temp.Chatbot_OnlineSupport__c;
        temp.OwnerId = ownerId;
        insert temp;
        return temp.Id;
    }
    
