    public AccountMaintainController() {
        routeOptions = new List<SelectOption>();
        selectedRoutesMap = new Map<String, Boolean>();
        isVisible = false;
        updateEmailSuccess = false;
        updateEmailError = false;
        
        
        String UserId = ApexPages.currentPage().getCookies().get('accountId').getValue();
        if (String.isNotBlank(UserId)) {
            List<UserManagement__c> UserList = [SELECT Id,Name,Salutation__c,State__c, CN_Passport__c, Account__c, PassWord__c, Email__c,Email1__c,Email2__c,Email3__c,
                                                	   Email4__c,Email5__c,EmailMemo1__c,EmailMemo2__c,EmailMemo3__c,EmailMemo4__c,EmailMemo5__c, Verify__c, ExpireDate__c, IATA_MSO_Code__c, 
                                                	   EN_Passport__c, Phone1__c, Phone2__c, Route__c,Role__c, Company__c,Company__r.Name,Prefix1__c,Prefix2__c,CreatedDate,LastModifiedDate,
                                                	   LastModifiedBy__c, Createdby__c, Password2__c, Password3__c
                                                FROM UserManagement__c WHERE Id = :UserId LIMIT 1];
            if (!UserList.isEmpty()) {
                User = UserList[0];
                // 創建新的變數來儲存加 8 小時後的時間
                adjustedCreatedDate = User.CreatedDate.addHours(8);
                adjustedLastModifiedDate = User.LastModifiedDate.addHours(8);
                system.debug(adjustedCreatedDate);
                system.debug(adjustedLastModifiedDate);
                
            }
            
        }
        
        // 獲取 UserManagement__c 物件的 Route__c 欄位的描述
        Schema.DescribeFieldResult fieldResult = UserManagement__c.Route__c.getDescribe();
        
        // 獲取所有可能的選項
        List<Schema.PicklistEntry> ple = fieldResult.getPicklistValues();
        
        Integer index = 1;
        for(Schema.PicklistEntry entry : ple) {
            String optionValue = String.valueOf(index) + '.' + entry.getLabel();
            routeOptions.add(new SelectOption(optionValue, entry.getLabel()));
            selectedRoutesMap.put(optionValue, false);
            index++;
        }
        
        // 設置當前選中的路線
        if(User.Route__c != null) {
            List<String> currentRoutes = User.Route__c.split(';');
            for(String route : currentRoutes) {
                for(SelectOption option : routeOptions) {
                    if(option.getLabel() == route) {
                        selectedRoutesMap.put(option.getValue(), true);
                        break;
                    }
                }
            }
        }
        
    }
    
    public void updateSelectedRoutes() {
        List<String> selectedRoutes = new List<String>();
        for(String key : selectedRoutesMap.keySet()) {
            if(selectedRoutesMap.get(key)) {
                selectedRoutes.add(key.substringAfter('.'));
            }
        }
        User.Route__c = String.join(selectedRoutes, ';');
    }
    
    
    
    
    
    @RemoteAction
    public static Map<String, Object> resetPassword(String newPassword, String confirmPassword, String accountId) {
        Map<String, Object> response = new Map<String, Object>();
        try {
            System.debug('接收到的數據: ' + newPassword + confirmPassword + accountId);
            UserManagement__c user = UserUtilities.getUserById(accountId);
            
            if (user == null) {
                response.put('status', false);
                response.put('errorType', 'USER_NOT_FOUND');
                response.put('message', '找不到使用者');
                return response;
            }
            
            if (newPassword.equals(user.PassWord__c)||newPassword.equals(user.PassWord2__c)||newPassword.equals(user.PassWord3__c)) {
                response.put('status', false);
                response.put('errorType', 'SAME_PASSWORD');
                response.put('message', '新密碼不可與前三次密碼相同');
                return response;
            }
    
            // 查詢 B2B_Parameter__c 的 Value__c
            Integer daysToAdd = 90; // 預設值
            try {
                B2B_Parameter__c parameter = [SELECT Value__c FROM B2B_Parameter__c WHERE Name = 'P003' LIMIT 1];
                if (parameter != null && String.isNotBlank(parameter.Value__c)) {
                    daysToAdd = Integer.valueOf(parameter.Value__c);
                }
            } catch (Exception e) {
                System.debug('無法取得參數 P003：' + e.getMessage());
            }
    
            // 設定新密碼和過期日期
            user.PassWord__c = newPassword;
            user.ExpireDate__c = Date.today().addDays(daysToAdd);
            update user;
    
            response.put('status', true);
            response.put('errorType', null); // 無錯誤
            response.put('message', '密碼重置成功');
            return response;
        } catch (Exception e) {
            response.put('status', false);
            response.put('errorType', 'EXCEPTION');
            response.put('message', '系統錯誤：' + e.getMessage());
            return response;
        }
    }


    
    

    
    
    

    
    
    @RemoteAction
    public static String saveEmailData(String emailDataJson) {
        try {
            System.debug('接收到的數據: ' + emailDataJson);
            
            // 將JSON字串反序列化為Map
            Map<String, Object> emailData = (Map<String, Object>)JSON.deserializeUntyped(emailDataJson);
            
            // 從傳入的數據中獲取 accountId
            String userId = (String)emailData.get('accountId');
            
            UserManagement__c user = UserUtilities.getUserById(userId);
            
            if (user == null) {
                throw new AuraHandledException('找不到指定的用戶資料');
            }
            
            // 查詢並更新記錄
            UserManagement__c userToUpdate = [SELECT Id FROM UserManagement__c WHERE Id = :userId LIMIT 1];
            
            // 更新Email相關欄位
            userToUpdate.Email1__c = (String)emailData.get('email1');
            userToUpdate.EmailMemo1__c = (String)emailData.get('emailMemo1');
            userToUpdate.Email2__c = (String)emailData.get('email2');
            userToUpdate.EmailMemo2__c = (String)emailData.get('emailMemo2');
            userToUpdate.Email3__c = (String)emailData.get('email3');
            userToUpdate.EmailMemo3__c = (String)emailData.get('emailMemo3');
            userToUpdate.Email4__c = (String)emailData.get('email4');
            userToUpdate.EmailMemo4__c = (String)emailData.get('emailMemo4');
            userToUpdate.Email5__c = (String)emailData.get('email5');
            userToUpdate.EmailMemo5__c = (String)emailData.get('emailMemo5');
            
            // 執行更新
            update userToUpdate;
            
            return 'Success';
            
        } catch (Exception e) {
            System.debug('Error: ' + e.getMessage());
            // 不要返回錯誤訊息，而是重新拋出異常
            throw new AuraHandledException(e.getMessage());
        }
    }
    
