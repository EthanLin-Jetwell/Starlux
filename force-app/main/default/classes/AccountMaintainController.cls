    public List<SelectOption> getCountryCodeOptions() {
        List<SelectOption> options = new List<SelectOption>();
        
        List<CountryCode__c> countryCodes = [SELECT CountryCode__c, CN_Name__c FROM CountryCode__c WHERE CountryCode__c != null];
        
        for(CountryCode__c cc : countryCodes) {
            // 組合顯示格式："+886 台灣" 這樣的格式
            String label = cc.CountryCode__c + ' ' + cc.CN_Name__c;
            options.add(new SelectOption(cc.CountryCode__c, label));
        }
        
        return options;
    }
    
    
    @RemoteAction
    public static String saveUserData(String UserDataJson) {
        try {
            System.debug('接收到的數據: ' + UserDataJson);
            
            Map<String, Object> UserData = (Map<String, Object>)JSON.deserializeUntyped(UserDataJson);
             

            
            
            // 從傳入的數據中獲取 accountId
            String UserId = (String)UserData.get('accountId');
            UserManagement__c user = UserUtilities.getUserById(UserId);
            
            if (user == null) {
                throw new AuraHandledException('找不到指定的用戶資料');
            }
            
            UserManagement__c[] existingUsers = [
                SELECT Id, Name, CN_Passport__c 
                FROM UserManagement__c 
                WHERE Id = :UserId 
                LIMIT 1
            ];
            
            // 查詢並更新記錄
            UserManagement__c UserToUpdate = [SELECT Id FROM UserManagement__c WHERE Id = :UserId LIMIT 1];
            
            String routeVal = normalizeRouteValue((String)UserData.get('routeValue'));
            UserToUpdate.Email__c = (String)UserData.get('email');
            UserToUpdate.Salutation__c = (String)UserData.get('title');
            UserToUpdate.CN_Passport__c = (String)UserData.get('chineseName');
            UserToUpdate.EN_Passport__c = (String)UserData.get('englishName');
            UserToUpdate.Phone1__c = (String)UserData.get('phoneNumber1');
            UserToUpdate.Phone2__c = (String)UserData.get('phoneNumber2');
            UserToUpdate.Prefix1__c = (String)UserData.get('editPCode1');
            UserToUpdate.Prefix2__c = (String)UserData.get('editPCode2');
            UserToUpdate.Route__c = String.isBlank(routeVal) ? null : routeVal;
            
            system.debug(existingUsers[0].CN_Passport__c);
            if (!existingUsers.isEmpty()) {
                UserToUpdate.Createdby__c = existingUsers[0].CN_Passport__c;
                UserToUpdate.LastModifiedBy__c = existingUsers[0].CN_Passport__c;
            }
            
            update UserToUpdate;
            return 'Success';
            
        } catch (Exception e) {
            System.debug('Error: ' + e.getMessage());
            // 不要返回錯誤訊息，而是重新拋出異常
            throw new AuraHandledException(e.getMessage());
        }
    }
    public static String normalizeRouteValue(String val) {
        if (String.isBlank(val)) return null;
        val = val.trim();
        if (val == '請選擇') return null;
        return val;
    }
