    @RemoteAction
    public static AvailabilityResult checkAvailability(String account, String email, Id excludeId) {
        AvailabilityResult res = new AvailabilityResult();
        account = String.isBlank(account) ? null : account.trim();
        email   = String.isBlank(email)   ? null : email.trim();
        if (account == null && email == null) return res;
    
        if (account != null) {
            res.accountTaken = (excludeId == null)
                ? ([SELECT COUNT() FROM UserManagement__c WHERE Account__c = :account] > 0)
                : ([SELECT COUNT() FROM UserManagement__c WHERE Account__c = :account AND Id != :excludeId] > 0);
        }
        if (email != null) {
            res.emailTaken = (excludeId == null)
                ? ([SELECT COUNT() FROM UserManagement__c WHERE Email__c = :email] > 0)
                : ([SELECT COUNT() FROM UserManagement__c WHERE Email__c = :email AND Id != :excludeId] > 0);
        }
        return res;
    }

