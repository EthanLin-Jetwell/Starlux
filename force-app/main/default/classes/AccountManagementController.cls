    private void loadAllUser() {
        // 查詢所有符合條件的記錄
        Id targetCompanyId = [
            SELECT Company__c
            FROM UserManagement__c
            WHERE Id = :UserId
            LIMIT 1
        ].Company__c;
        allUserRecords = [SELECT Id, Name,CN_Passport__c, EN_Passport__c, Phone1__c, Phone2__c, 
                          Email__c, Route__c, Prefix1__c, Prefix2__c, Company__c
                          FROM UserManagement__c 
                          WHERE Role__c = '1' 
                          AND Id != :UserId
                          AND Company__c = :targetCompanyId
                          ORDER BY CN_Passport__c];
        
        // 計算總筆數與總頁數
        totalRecords = allUserRecords.size();
        totalPages = (Integer)Math.ceil(totalRecords / (Decimal)pageSize);
        if(totalPages == 0) totalPages = 1;
        
        // 更新當前頁面的記錄
        updatePageRecords();
    }
    
    private void updatePageRecords() {
        // 計算當前頁面的起始和結束索引
        Integer startIndex = (currentPage - 1) * pageSize;
        Integer endIndex = Math.min(startIndex + pageSize, allUserRecords.size());
        
        // 計算當前頁面的記錄筆數
        startRecord = startIndex + 1;
        endRecord = endIndex;
        
        // 獲取當前頁面的記錄
        UserList = new List<UserManagement__c>();
        for(Integer i = startIndex; i < endIndex; i++) {
            UserList.add(allUserRecords[i]);
        }
    }
    
    // 下一頁方法
    public void nextPage() {
        if(currentPage < totalPages) {
            currentPage++;
            updatePageRecords();
        }
    }
    
    // 上一頁方法
    public void previousPage() {
        if(currentPage > 1) {
            currentPage--;
            updatePageRecords();
        }
    }
    

    
    
    public void searchUser() {
        // 1) 先取「上面正在編輯那筆 UserManagement__c」的公司 Id
        //    注意：這裡的 UserId 不是 Salesforce User.Id，而是你那筆 UserManagement__c 的 Id
        Id targetCompanyId = [
            SELECT Company__c
            FROM UserManagement__c
            WHERE Id = :UserId
            LIMIT 1
        ].Company__c;
    
        // 2) 組條件：同公司 + 不含自己
        List<String> conditions = new List<String>();
        conditions.add('Company__c = :targetCompanyId');
        conditions.add('Id != :UserId');
        
        if (String.isNotBlank(searchCName)) {
            System.debug('searchCName: ' + searchCName);
            conditions.add('CN_Passport__c LIKE \'%' + String.escapeSingleQuotes(searchCName) + '%\'');
        }
        if (String.isNotBlank(searchEName)) {
            System.debug('searchEName: ' + searchEName);
            conditions.add('EN_Passport__c LIKE \'%' + String.escapeSingleQuotes(searchEName) + '%\'');
        }
        if (String.isNotBlank(searchRoute)) {
            System.debug('searchRoute: ' + searchRoute);
            conditions.add('Route__c INCLUDES (\'' + String.escapeSingleQuotes(searchRoute) + '\')');
        }
        
        conditions.add('Role__c = \'1\'');
        
        String query = 'SELECT Id, Name,CN_Passport__c, EN_Passport__c, Phone1__c, Phone2__c, ' +
            'Email__c, Route__c, Prefix1__c, Prefix2__c ' +
            'FROM UserManagement__c';
        
        if (!conditions.isEmpty()) {
            query += ' WHERE ' + String.join(conditions, ' AND ');
        }
        
        System.debug('Generated Query: ' + query);
        
        try {
            // 將查詢結果存儲在 allUserRecords 中，而不是直接存在 UserList
            allUserRecords = Database.query(query);
            
            // 重置當前頁碼到第一頁
            currentPage = 1;
            
            // 重新計算總頁數
            totalPages = (Integer)Math.ceil(allUserRecords.size() / (Decimal)pageSize);
            if(totalPages == 0) totalPages = 1;
            
            // 更新當前頁面顯示的記錄
            updatePageRecords();
            
        } catch (Exception e) {
            System.debug('Error during query: ' + e.getMessage());
            ApexPages.addMessage(new ApexPages.Message(
                ApexPages.Severity.ERROR, 
                'An error occurred while searching: ' + e.getMessage()
            ));
        }
    }
    
    public void updateSearchRoute() {
        List<String> selectedRoutes = new List<String>();
        for (String key : selectedRoutesMap.keySet()) {
            if (selectedRoutesMap.get(key)) {
                selectedRoutes.add(key);
            }
        }
        searchRoute = String.join(selectedRoutes, ', ');
    }
    
    
    public void clearSearch() {
        searchCName = '';
        searchEName = '';
        searchRoute = '';
        currentPage = 1; // 重置到第一頁
    
        // 清除選擇的航線映射
        for (String key : selectedRoutesMap.keySet()) {
            selectedRoutesMap.put(key, false);
        }
    
        loadAllUser();
    }

	
    


    
