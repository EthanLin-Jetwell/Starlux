        selectedRoutesMap = new Map<String, Boolean>();
        newUser = new UserManagement__c();
        totalRecords = allUserRecords.size();
        
        // 獲取 UserManagement__c 物件的 Route__c 欄位的描述
        Schema.DescribeFieldResult fieldResult = UserManagement__c.Route__c.getDescribe();
        
        // 獲取所有可能的選項
        List<Schema.PicklistEntry> ple = fieldResult.getPicklistValues();
        
        Integer index = 1;
        for(Schema.PicklistEntry entry : ple) {
            String optionValue = String.valueOf(index) + '.' + entry.getLabel();
            routeOptions.add(new SelectOption(optionValue, entry.getLabel()));
            selectedRoutesMap.put(optionValue, false);
            index++;
        }
    }

    
    
    public List<SelectOption> getCountryCodeOptions() {
        List<SelectOption> options = new List<SelectOption>();
        
        List<CountryCode__c> countryCodes = [SELECT CountryCode__c, CN_Name__c FROM CountryCode__c WHERE CountryCode__c != null];
        
        for(CountryCode__c cc : countryCodes) {
            // 組合顯示格式："+886 台灣" 這樣的格式
            String label = cc.CountryCode__c + ' ' + cc.CN_Name__c;
            options.add(new SelectOption(cc.CountryCode__c, label));
        }
        
        return options;
    }
    
