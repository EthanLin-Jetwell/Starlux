    @RemoteAction
    public static String createNewUser(Map<String, Object> formData) {
        System.debug('開始建立新員工記錄');
        System.debug('接收到的表單資料: ' + formData);
        
        Boolean addsuccess = false;
        Boolean adderror = false;
        
        Savepoint sp = Database.setSavepoint();
        
        try {
            String nowUserID = (String)formData.get('nowUser');
            String newAccountValue = (String)formData.get('username'); // 取得新的Account__c值
            
            // 檢查現有用戶和Account__c是否重複
            UserManagement__c[] existingUsers = [
                SELECT Id, Name, CN_Passport__c, Account__c, Company__c
                FROM UserManagement__c 
                WHERE Account__c = :newAccountValue
                LIMIT 1
            ];
            // 檢查是否有重複的Account__c
            if (!existingUsers.isEmpty()) {
                throw new AuraHandledException('帳號已存在，請使用其他帳號');
            }
            
            UserManagement__c newUser = new UserManagement__c();
            
            newUser.Account__c = newAccountValue;
            newUser.Email__c = (String)formData.get('email');
            newUser.CN_Passport__c = (String)formData.get('newCName');
            newUser.EN_Passport__c = (String)formData.get('newEName');
            newUser.Phone1__c = (String)formData.get('phone1');
            newUser.Phone2__c = (String)formData.get('phone2');
            newUser.Route__c = (String)formData.get('Route');
            newUser.Salutation__c = (String)formData.get('title');
            newUser.Resend__c = true;
            string roleValue = (String)formData.get('Role');
            if (roleValue == '管理者') {
                newUser.Role__c = '0';
            } else if (roleValue == '領隊/導遊') {
                newUser.Role__c = '1';
            }
            newUser.Memo__c = (String)formData.get('Memo');
            newUser.Prefix1__c = (String)formData.get('CountryCode1');
            newUser.Prefix2__c = (String)formData.get('CountryCode2');
            
            // 設置創建者和修改者
            for(UserManagement__c user : existingUsers) {
                if(user.Id == nowUserID) {
                    newUser.Company__c = user.Company__c;
                    newUser.Createdby__c = user.CN_Passport__c;
                    newUser.LastModifiedBy__c = user.CN_Passport__c;
                    break;
                }
            }
            
            System.debug('準備插入的資料: ' + newUser);
            insert newUser;
            System.debug('新員工記錄建立成功，ID: ' + newUser.Id);
            addsuccess = true;
            return 'success';
            
        } catch (Exception e) {
            System.debug(LoggingLevel.ERROR, '錯誤: ' + e.getMessage());
            Database.rollback(sp);
            throw new AuraHandledException('新增失敗：' + e.getMessage());
        }
    }
    
    
    


 
    
