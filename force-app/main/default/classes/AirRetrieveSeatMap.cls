public class AirRetrieveSeatMap {
    public class Result {
        public String iataAircraftTypeCode { get; set;}
        public Set<String> cabinClasses { get; set;}
        public Set<String> blockedSeats { get; set;}
        
        public Result() {
            cabinClasses = new Set<String>();
            blockedSeats = new Set<String>();
        }
    }

    public static Result analyzeXml(String xmlBody) {
        Result result = new Result();

        if (String.isBlank(xmlBody)) {
            System.debug('❌ XML is empty.');
            return result;
        }

        Dom.Document doc = new Dom.Document();
        try {
            doc.load(xmlBody);
        } catch (Exception e) {
            System.debug('❌ Error parsing XML: ' + e.getMessage());
            return result;
        }

        Dom.XmlNode root = doc.getRootElement();
        Dom.XmlNode body = findChildByName(root, 'Body');
        if (body == null) return result;

        // 同時支援 Air_RetrieveSeatMapReply 和 DCSSTG_GetFDSeatMapReply
        Dom.XmlNode reply = findChildByName(body, 'Air_RetrieveSeatMapReply');
        if (reply == null) {
            reply = findChildByName(body, 'DCSSTG_GetFDSeatMapReply');
        }
        if (reply == null) return result;

        Dom.XmlNode seatmapInfo = findChildByName(reply, 'seatmapInformation');
        if (seatmapInfo == null) return result;

        // 機型代碼
        Dom.XmlNode equipmentInfo = findChildByName(seatmapInfo, 'equipmentInformation');
        if (equipmentInfo != null) {
            Dom.XmlNode aircraftCode = findChildByName(equipmentInfo, 'iataAircraftTypeCode');
            if (aircraftCode != null) {
                result.iataAircraftTypeCode = aircraftCode.getText();
            }
        }
		/*
        // 艙等代碼
        for (Dom.XmlNode cabin : seatmapInfo.getChildElements()) {
            if (cabin.getName().endsWith('cabin')) {
                for (Dom.XmlNode compartment : cabin.getChildElements()) {
                    if (compartment.getName().endsWith('compartmentDetails')) {
                        for (Dom.XmlNode detail : compartment.getChildElements()) {
                            if (detail.getName().endsWith('classDetails')) {
                                Dom.XmlNode classNode = findChildByName(detail, 'classDesignator');
                                if (classNode != null) {
                                    result.cabinClasses.add(classNode.getText());
                                }
                            }
                        }
                    }
                }
            }
        }
		*/
        // 被封鎖的座位
        Set<String> blockedOccupationCodes = new Set<String>{ 'P','H','D','W','T','K','Z','O' };
        Set<String> blockedCharacteristics = new Set<String>{ 'CH' };
        Map<String, String> blockedReasons = new Map<String, String>();
        
        for (Dom.XmlNode rowNode : seatmapInfo.getChildElements()) {
            if (rowNode.getName().endsWith('row')) {
                Dom.XmlNode rowDetail = findChildByName(rowNode, 'rowDetails');
                if (rowDetail == null) continue;
        
                Dom.XmlNode rowNumNode = findChildByName(rowDetail, 'seatRowNumber');
                if (rowNumNode == null) continue;
                String rowNum = rowNumNode.getText();
        
                for (Dom.XmlNode seatDetail : rowDetail.getChildElements()) {
                    if (seatDetail.getName().endsWith('seatOccupationDetails')) {
                        String column = null;
                        Boolean isBlocked = false;
                        List<String> reasons = new List<String>();
        
                        for (Dom.XmlNode child : seatDetail.getChildElements()) {
                            String name = child.getName();
        
                            if (name.endsWith('seatColumn')) {
                                column = child.getText();
                            } else if (name.endsWith('seatOccupation')) {
                                if (blockedOccupationCodes.contains(child.getText())) {
                                    isBlocked = true;
                                    reasons.add('[佔用:' + child.getText() + ']');
                                }
                            } else if (name.endsWith('seatCharacteristic')) {
                                if (blockedCharacteristics.contains(child.getText())) {
                                    isBlocked = true;
                                    reasons.add('[特性:' + child.getText() + ']');
                                }
                            }
                        }
        
                        if (isBlocked && column != null) {
                            String seatCode = rowNum + column;
                            result.blockedSeats.add(seatCode);
                            blockedReasons.put(seatCode, String.join(reasons, ' '));
                        }
                    }
                }
            }
        }
        for (String seat : blockedReasons.keySet()) {
            System.debug(':no_entry: 禁用座位 ' + seat + ' ➜ 原因：' + blockedReasons.get(seat));
        }
        return result;
    }

    // 忽略 namespace 的安全子節點搜尋
    private static Dom.XmlNode findChildByName(Dom.XmlNode parent, String localName) {
        for (Dom.XmlNode child : parent.getChildElements()) {
            if (child.getName() == localName || child.getName().endsWith(':' + localName)) {
                return child;
            }
        }
        return null;
    }
    
