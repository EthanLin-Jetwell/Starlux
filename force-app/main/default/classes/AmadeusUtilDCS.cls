    public static String convertDDMMYYtoYYYYMMDD(String input) {
        if (input == null || input.length() != 6) {
            return null;
        }
    
        String dd = input.substring(0, 2);
        String mm = input.substring(2, 4);
        String yy = input.substring(4, 6);
    
        Integer inputYear = Integer.valueOf(yy);
        Integer currentYear = Integer.valueOf(Date.today().year());
        Integer currentYY = Math.mod(currentYear,100);
    
        String yyyy = (inputYear >= currentYY) ? ('20' + yy) : ('21' + yy);
        return yyyy + mm + dd;
    }
    
