public class B2BForgotPasswordController {
    public String email {get; set;}
    public String IATAcode {get; set;}
    public String password {get; set;}
    public String UserAccount {get; set;}
    public String errorMessage {get; set;}
    public Boolean isVisible {get; set;}
    public Integer minutes {get; set;}
    public Boolean isWrongCredentials { get; set; }
    public Boolean isAccountInactive { get; set; }
    public Boolean isResetPassword { get; set; }
    public String accountId { get; set; }
    //public Boolean isPersonIDMismatch = false;
    
    public B2BForgotPasswordController() {
        isVisible = false;
    }
	
    public Pagereference sendEmail() {
        // 重置狀態
        isWrongCredentials = false;
        isAccountInactive = false;
        isResetPassword = false;
        //isPersonIDMismatch = false;
        accountId = null;
        
        // 查詢符合 UserName 、 IATAcode 和 Email 的員工資料
        UserManagement__c acc = UserUtilities.getUserByAccountAndIATAcode(UserAccount, IATAcode, email);
        system.debug(acc);
        
        
        //system.debug(accountId);
        
        // 檢查是否存在此帳號跟統編
        if (acc == null) {
            isWrongCredentials = true;
            return null;
        }
        
                // 設置 accountId
        accountId = acc.Id;
        
        // 檢查帳號是否開通
        if (!acc.Verify__c) {
            //isAccountInactive = true;  
            acc.Resend__c = true;    
            update acc;
        }
        
        LoginUtility.saveLoginCookie(acc.Id);
        
        isResetPassword = true;
        return null;
    }
    
    
    public Pagereference redirectLogin() {
        return PageUtility.toPage(UrlConstants.LOGIN);
    }

