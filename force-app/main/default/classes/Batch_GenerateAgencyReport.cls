    global Batch_GenerateAgencyReport() {
        RecordType groupBookingRecordType = [SELECT Id FROM RecordType WHERE SObjectType = 'Case' AND DeveloperName = 'Group_Booking' LIMIT 1];
        groupBookingRecordTypeId = groupBookingRecordType.Id;
        startD = System.today().toStartOfMonth().addMonths(-1);
        endD = System.today().toStartOfMonth().addMonths(5);
        
        this.query = 'SELECT Id, AccountId, Route__c, F_Departing__c, F_Returning__c, Total_Final_Allocated_Seats__c, Total_Seat_Sales__c, Returning_AU__r.AU__c, Departing_Flight_Date__c, Total_Seat_Sales_Real__c, Returning_Flight_Date__c,' +
                '(SELECT Id, Total_Final_Allocated_Seats__c, Total_Seat_Sales__c FROM Case_Agency_Reports__r ORDER BY CreatedDate DESC LIMIT 1) ' +
                'FROM Case WHERE RecordTypeId =: groupBookingRecordTypeId AND ((F_Departing__c != NULL AND Departing_Flight_Date__c >= : startD AND Departing_Flight_Date__c < : endD) OR (F_Returning__c != NULL AND Returning_Flight_Date__c >= : startD AND Returning_Flight_Date__c < : endD))';
        
        if(Test.isRunningTest()) {
            this.query = 'SELECT Id, AccountId, Route__c, F_Departing__c, F_Returning__c, Total_Final_Allocated_Seats__c, Total_Seat_Sales__c, Returning_AU__r.AU__c, Departing_Flight_Date__c, Total_Seat_Sales_Real__c, Returning_Flight_Date__c,' +
            '(SELECT Id, Total_Final_Allocated_Seats__c, Total_Seat_Sales__c FROM Case_Agency_Reports__r ORDER BY CreatedDate DESC LIMIT 1) ' +
            'FROM Case WHERE RecordTypeId =: groupBookingRecordTypeId LIMIT 1';
        }
    }
    
