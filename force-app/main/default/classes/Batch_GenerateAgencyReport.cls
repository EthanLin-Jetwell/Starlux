    global void execute(Database.BatchableContext bc, List < SObject > records) {
        List<AgencyReport__c> insertList = new List<AgencyReport__c>();
        List<AirportCode__c> airportCodes = new List<AirportCode__c>();
        System.debug(records);
        List<String> waypoints = new List<String>();
        for(SObject obj : records) {
            Case temp = (Case)obj;
            System.debug(temp.F_Departing__c);
            List<String> depStrList = temp.F_Departing__c.split('-');
            waypoints.add(depStrList[1]);
            List<String> retStrList = temp.F_Returning__c.split('-');
            waypoints.add(retStrList[0]);
        }
        
        airportCodes = [SELECT Id, Area__c, WaypointTaiwan__c, WaypointOutside__c, Order__c FROM AirportCode__c WHERE WaypointOutside__c IN: waypoints AND Active__c = true];
            
        Map<String, String> areaMap = new Map<String, String>();
        Map<String, Decimal> orderMap = new Map<String, Decimal>();
        
        for(AirportCode__c airportCode : airportCodes) {
            areaMap.put(airportCode.WaypointOutside__c, airportCode.Area__c);
            orderMap.put(airportCode.WaypointTaiwan__c + '-' + airportCode.WaypointOutside__c, airportCode.Order__c);
        }
        
        System.debug(areaMap);
        
        Set<String> groupWaypointNameList = new Set<String>();
        Set<String> groupAgencyNameList = new Set<String>();
        
        Group_By_Waypoint__c tempGW = new Group_By_Waypoint__c();
        insert tempGW;
        
        Group_By_Agency__c tempGA = new Group_By_Agency__c();
        insert tempGA;
        
        //Add by Jeremy 20240607 Begin
		/*List<String> TmpDeparting = new List<String>();
		for(SObject obj : records) {
            Case temp = (Case)obj;
            if(temp.Route__c != NULL){
                TmpDeparting.add('%' + temp.Route__c + '%');
            }
        }
        Map<String, Decimal> MapAU = new Map<String, Decimal>();
        FOR(Group_AU__c GAU : [select Au__c, F_Departing__c, F_Returning__c,Flight_Date__c from Group_AU__c where Route__c like :TmpDeparting]) MapAU.put(GAU.Route__c+'-'+GAU.Flight_Date__c,GAU.Au__c);
		System.debug(MapAU);
        Map<String, Decimal> MapNameToAU = new Map<String, Decimal>();*/
        //Add by Jeremy 20240607 End
        
        for(SObject obj : records) {
            Case temp = (Case)obj;
            
            //Add by Jeremy 20240607 Begin
            //Decimal AU_value = 0;
            //if(MapAU.containskey(temp.Route__c+'-'+temp.Departing_Flight_Date__c)) AU_value = MapAU.get(temp.Route__c+'-'+temp.Departing_Flight_Date__c);
            //Add by Jeremy 20240607 End
            
            if(temp.F_Departing__c != NULL && temp.Departing_Flight_Date__c != NULL) {
                AgencyReport__c agencyReport = new AgencyReport__c();            
                List<String> strDepList = temp.F_Departing__c.split('-');
                List<String> strRetList = temp.F_Departing__c.split('-');
                
                if(temp.Total_Final_Allocated_Seats__c != NULL) agencyReport.Total_Final_Allocated_Seats__c = temp.Total_Final_Allocated_Seats__c;
                if(temp.Total_Seat_Sales_Real__c != NULL) agencyReport.Total_Seat_Sales__c = temp.Total_Seat_Sales_Real__c;
                agencyReport.WaypointTaiwan__c = strDepList[0];
                agencyReport.WaypointOutside__c = strDepList[1];
                if(temp.Departing_Flight_Date__c != NULL) agencyReport.Departing_Flight_Date__c = temp.Departing_Flight_Date__c;
                if(temp.Departing_Flight_Date__c != NULL) agencyReport.Year__c = String.valueOf(temp.Departing_Flight_Date__c.year());
                if(temp.Departing_Flight_Date__c != NULL) agencyReport.Month__c = String.valueOf(temp.Departing_Flight_Date__c.month());
                if(temp.Case_Agency_Reports__r.size() != 0) agencyReport.Total_Final_Allocated_Seats_Y__c = temp.Case_Agency_Reports__r[0].Total_Final_Allocated_Seats__c;
                if(temp.Case_Agency_Reports__r.size() != 0) agencyReport.Total_Seat_Sales_Y__c = temp.Case_Agency_Reports__r[0].Total_Seat_Sales__c;
                agencyReport.Case__c = temp.Id;
                agencyReport.Account__c = temp.AccountId;
                
                agencyReport.Group_By_Waypoint__c = tempGW.Id;
                agencyReport.Group_By_Agency__c = tempGA.Id;
                
                if(areaMap.keySet().contains(agencyReport.WaypointOutside__c)) {
                    agencyReport.Area__c = areaMap.get(agencyReport.WaypointOutside__c);
                    insertList.add(agencyReport);
                }
                if(!groupWaypointNameList.contains(agencyReport.Year__c + '-' + agencyReport.Month__c + '-' + agencyReport.WaypointTaiwan__c + '-' + agencyReport.WaypointOutside__c)) {
                    groupWaypointNameList.add(agencyReport.Year__c + '-' + agencyReport.Month__c + '-' + agencyReport.WaypointTaiwan__c + '-' + agencyReport.WaypointOutside__c);
                }
                if(!groupAgencyNameList.contains(agencyReport.Year__c + '-' + agencyReport.Month__c + '-' + agencyReport.WaypointTaiwan__c + '-' +  agencyReport.WaypointOutside__c + '-' + agencyReport.Account__c)) {
                    groupAgencyNameList.add(agencyReport.Year__c + '-' + agencyReport.Month__c + '-' + agencyReport.WaypointTaiwan__c + '-' +  agencyReport.WaypointOutside__c + '-' + agencyReport.Account__c);
                }
            }
            //New code
            if(temp.F_Returning__c != NULL && temp.Returning_Flight_Date__c != NULL) {
                AgencyReport__c agencyReport = new AgencyReport__c();            
                List<String> strRetList = temp.F_Returning__c.split('-');
                
                if(temp.Total_Final_Allocated_Seats__c != NULL) agencyReport.Total_Final_Allocated_Seats__c = temp.Total_Final_Allocated_Seats__c;
                if(temp.Total_Seat_Sales_Real__c != NULL) agencyReport.Total_Seat_Sales__c = temp.Total_Seat_Sales_Real__c;
                agencyReport.WaypointTaiwan__c = strRetList[1];
                agencyReport.WaypointOutside__c = strRetList[0];
                // agencyReport.Departing_Flight_Date__c
                if(temp.Returning_Flight_Date__c != NULL) agencyReport.Departing_Flight_Date__c = temp.Returning_Flight_Date__c;
                if(temp.Returning_Flight_Date__c != NULL) agencyReport.Year__c = String.valueOf(temp.Returning_Flight_Date__c.year());
                if(temp.Returning_Flight_Date__c != NULL) agencyReport.Month__c = String.valueOf(temp.Returning_Flight_Date__c.month());
                if(temp.Case_Agency_Reports__r.size() != 0) agencyReport.Total_Final_Allocated_Seats_Y__c = temp.Case_Agency_Reports__r[0].Total_Final_Allocated_Seats__c;
                if(temp.Case_Agency_Reports__r.size() != 0) agencyReport.Total_Seat_Sales_Y__c = temp.Case_Agency_Reports__r[0].Total_Seat_Sales__c;
                agencyReport.Case__c = temp.Id;
                agencyReport.Account__c = temp.AccountId;
                
                agencyReport.Group_By_Waypoint__c = tempGW.Id;
                agencyReport.Group_By_Agency__c = tempGA.Id;
                
                if(areaMap.keySet().contains(agencyReport.WaypointOutside__c)) {
                    agencyReport.Area__c = areaMap.get(agencyReport.WaypointOutside__c);
                    insertList.add(agencyReport);
                }
                
                if(!groupWaypointNameList.contains(agencyReport.Year__c + '-' + agencyReport.Month__c + '-' + agencyReport.WaypointTaiwan__c + '-' + agencyReport.WaypointOutside__c)) {
                    groupWaypointNameList.add(agencyReport.Year__c + '-' + agencyReport.Month__c + '-' + agencyReport.WaypointTaiwan__c + '-' + agencyReport.WaypointOutside__c);
                }
                if(!groupAgencyNameList.contains(agencyReport.Year__c + '-' + agencyReport.Month__c + '-' + agencyReport.WaypointTaiwan__c + '-' +  agencyReport.WaypointOutside__c + '-' + agencyReport.Account__c)) {
                    groupAgencyNameList.add(agencyReport.Year__c + '-' + agencyReport.Month__c + '-' + agencyReport.WaypointTaiwan__c + '-' +  agencyReport.WaypointOutside__c + '-' + agencyReport.Account__c);
                }
            }
        }
        
        System.debug(groupWaypointNameList);
        System.debug(groupAgencyNameList);
        List<Group_By_Waypoint__c> groupWaypointList = [SELECT Id, Name FROM Group_By_Waypoint__c WHERE Name IN : groupWaypointNameList AND CreatedDate = TODAY];
        List<Group_By_Agency__c> groupAgencyList = [SELECT Id, Name FROM Group_By_Agency__c WHERE Name IN : groupAgencyNameList AND CreatedDate = TODAY];
        
        /*List<Group_By_Waypoint__c> oldGroupWaypoints = new List<Group_By_Waypoint__c>();
        for (Group_By_Waypoint__c waypoint : groupWaypointList) {
            if(MapNameToAU.containsKey(waypoint.name)) {
                waypoint.Original_AU__c = MapNameToAU.get(waypoint.name);
                System.debug(waypoint.Original_AU__c);
            }
            oldGroupWaypoints.add(waypoint);
            groupWaypointNameList.remove(waypoint.Name);
        }*/

        for (Group_By_Waypoint__c waypoint : groupWaypointList) {
            groupWaypointNameList.remove(waypoint.Name);
        }
        
        List<Group_By_Waypoint__c> newGroupWaypoints = new List<Group_By_Waypoint__c>();

        for (String name : groupWaypointNameList) {
            Group_By_Waypoint__c newWaypoint = new Group_By_Waypoint__c();
            newWaypoint.Name = name;
            if(orderMap.keySet().contains(name.right(7))) {
                newWaypoint.Order__c = orderMap.get(name.right(7));
            } else {
                newWaypoint.Order__c = 0;
            }
            
            System.debug(newWaypoint.Name);
            /*if(MapNameToAU.containsKey(name)) {
                newWaypoint.Original_AU__c = MapNameToAU.get(name);
                System.debug(newWaypoint.Original_AU__c);
            }*/
            newGroupWaypoints.add(newWaypoint);
        }

        for (Group_By_Agency__c agency : groupAgencyList) {
            groupAgencyNameList.remove(agency.Name);
        }
        
        List<Group_By_Agency__c> newGroupAgencys = new List<Group_By_Agency__c>();

        for (String name : groupAgencyNameList) {
            Group_By_Agency__c newAgency = new Group_By_Agency__c();
            newAgency.Name = name;
            
            if(orderMap.keySet().contains(name.substring(7, 14))) {
                newAgency.Order__c = orderMap.get(name.substring(7, 14));
            } else if(orderMap.keySet().contains(name.substring(8, 15))) {
                newAgency.Order__c = orderMap.get(name.substring(8, 15));
            }else {
                newAgency.Order__c = 0;
            }
            
            newGroupAgencys.add(newAgency);
        }
        
        if(!newGroupWaypoints.isEmpty()) insert newGroupWaypoints;
        System.debug(newGroupAgencys);
        if(!newGroupAgencys.isEmpty()) insert newGroupAgencys;
        
        //if(!oldGroupWaypoints.isEmpty()) update oldGroupWaypoints;
        
        if(!insertList.isEmpty()) insert insertList;
    }
    
