    // 新增：公告內容私有屬性
    private String noticeContentBoardingPass;
    private String noticeContentEBoardingPass;
    
    // 公開 getter 方法
    public String getnoticeContentBoardingPass() {
        return noticeContentBoardingPass;
    }
    public String getnoticeContentEBoardingPass() {
        return noticeContentEBoardingPass;
    }
    
    // 新增：載入公告內容的方法
    private void loadnoticeContentBoardingPass() {
    try {
        List<NoticeSetting__c> notices = [
            SELECT Content__c FROM NoticeSetting__c
            WHERE Name = '紙本登機證 - 注意事項'
            LIMIT 1
        ];
        if (!notices.isEmpty() && notices[0].Content__c != null) {
            // 去除所有 HTML 標籤
            String rawContent = notices[0].Content__c;
            noticeContentBoardingPass = rawContent.replaceAll('<[^>]+>', '');
        } else {
            noticeContentBoardingPass = '';
        }
    } catch (Exception e) {
        noticeContentBoardingPass = '';
        System.debug('Error loading notice content: ' + e.getMessage());
    }
}

    
    private void loadnoticeContentEBoardingPass() {
        try {
            List<NoticeSetting__c> notices = [
                SELECT Content__c FROM NoticeSetting__c
                WHERE Name = '電子登機證 - 注意事項'
                LIMIT 1
            ];
            noticeContentEBoardingPass = (!notices.isEmpty() && notices[0].Content__c != null)
                ? notices[0].Content__c : '';
        } catch (Exception e) {
            noticeContentEBoardingPass = '';
            System.debug('Error loading notice content: ' + e.getMessage());
        }
    }
    
    /*──────────── Constructor ────────────*/
    public BoardingPassPDFController() {
        // 載入公告內容
        loadnoticeContentBoardingPass();
        loadnoticeContentEBoardingPass();
    }
    
    global void retrieveRecords() {
        String idsParam = ApexPages.currentPage().getParameters().get('ids');
        String recId    = ApexPages.currentPage().getParameters().get('id');
        String pnr      = ApexPages.currentPage().getParameters().get('pnr');
        //isSend = (ApexPages.currentPage().getParameters().get('issend') == 'true')? true : false;
            
        Set<Id> idSet = new Set<Id>();
        if (!String.isBlank(idsParam)) {
            for (String s : idsParam.split(',')) {
                if (!String.isBlank(s)) idSet.add((Id)s.trim());
            }
        } else if (!String.isBlank(recId)) {
            idSet.add((Id)recId);
        }
        
        String baseFields =
            'Id, CabinClass__c, Barcode_Formula__c, Barcode__c, ' +
            'FlightPrefix__c, FlightNumber__c, DepartureDate__c, ' +
            'DepartureLocation__c, Destination__c, ' +
            'LastName__c, FirstName__c, Title__c, ' +
            'BoardingGate__c, LatestBoardingTime__c, ' +
            'BoardingPriority__c, Seat__c, ' +
            'PNR__c, PAX__c, Meal__c, Service__c, DepartureTerminal__c,' +
            'SecurityNumber__c,FQTVTierCode__c,group_code__c'; // 2025-10-08 Erick
        
        List<TravellerInfo__c> raws;
        if (!idSet.isEmpty()) {
            raws = Database.query(
                'SELECT ' + baseFields +
                ' FROM   TravellerInfo__c WHERE Id IN :idSet ORDER BY Seat__c'
            );
        } else if (!String.isBlank(pnr)) {
            raws = Database.query(
                'SELECT ' + baseFields +
                ' FROM   TravellerInfo__c WHERE PNR__c = :pnr ORDER BY Seat__c'
            );
        } else {
            raws = new List<TravellerInfo__c>();
        }
        
        List<String> mAbbr = new List<String>{
            'JAN','FEB','MAR','APR','MAY','JUN',
                'JUL','AUG','SEP','OCT','NOV','DEC'
                };
                    
                    tickets = new List<TicketData>();
        List<TravellerInfo__c> updateList = new List<TravellerInfo__c>();
        for (TravellerInfo__c t : raws) {
            t.C4Finished__c = true;
            t.C4BoardingPassPrintedTime__c = System.now();
            updateList.add(t);
            
            TicketData d = new TicketData();
            d.cabinLabel    = t.CabinClass__c;
            d.barcodeBase64 = t.Barcode__c;
            
            String raw      = t.Barcode_Formula__c;
            String cleaned = raw != null
                ? raw.replaceAll('[\\x00-\\x1F\\x7F]+', ' ').trim()
                : '';
            d.barcodeTxt    = cleaned;
            
            String rawFlightNum = String.valueOf(t.FlightNumber__c).trim();
            while (rawFlightNum.length() < 4) {
                rawFlightNum = '0' + rawFlightNum;
            }
            d.flightNo = t.FlightPrefix__c + rawFlightNum;
            
            d.depAirport    = t.DepartureLocation__c;
            d.arrAirport    = t.Destination__c;
            d.zone          = t.BoardingPriority__c;
            d.seat          = t.Seat__c;
            String seqRaw = String.valueOf(t.SecurityNumber__c);
            if (!String.isBlank(seqRaw) && seqRaw.replaceAll('\\d','').length() == 0) {
                while (seqRaw.length() < 3) seqRaw = '0' + seqRaw;
            }
            if (String.isBlank(seqRaw)) seqRaw = '000';
            d.seq = 'SEQ ' + seqRaw;
            d.pnr           = t.PNR__c;
            d.fqtvTier      = t.FQTVTierCode__c; // 2025-07-29 Aaron
            d.checkppt = (t.group_code__c != 'OK') ? 'CHECK PPT' : null; // 2025-10-08 Erick
            d.etkt          = t.PAX__c;
            d.terminal      = t.DepartureTerminal__c;
            
            String dateRaw = String.valueOf(t.DepartureDate__c);
            d.depDate = '';
            if (!String.isBlank(dateRaw) && dateRaw.length() == 6) {
                try {
                    Integer day   = Integer.valueOf(dateRaw.substring(0, 2));
                    Integer month = Integer.valueOf(dateRaw.substring(2, 4));
                    if (month >= 1 && month <= 12) {
                        String dayStr   = (day < 10 ? '0' + day : String.valueOf(day));
                        String monthStr = mAbbr[month - 1];
                        d.depDate = dayStr + ' ' + monthStr;
                    }
                } catch (Exception e) {
                    System.debug('⚠️ 日期格式錯誤: ' + dateRaw);
                }
            }
            //EBoarding pass need year
            String dateRaw2 = String.valueOf(t.DepartureDate__c);
            d.depDate2 = '';
            if (!String.isBlank(dateRaw2) && dateRaw2.length() == 6) {
                try {
                    Integer day   = Integer.valueOf(dateRaw2.substring(0, 2));
                    Integer month = Integer.valueOf(dateRaw2.substring(2, 4));
                    Integer year  = 2000 + Integer.valueOf(dateRaw2.substring(4, 6));
                    if (month >= 1 && month <= 12) {
                        String dayStr   = (day < 10 ? '0' + day : String.valueOf(day));
                        String monthStr = mAbbr[month - 1];
                        d.depDate2 = dayStr + ' ' + monthStr + ' ' + String.valueOf(year);
                    }
                } catch (Exception e) {
                    System.debug('⚠️ 日期格式錯誤: ' + dateRaw);
                }
            }
            
            //d.paxName = (t.LastName__c + '/' + t.FirstName__c +
                        // (String.isBlank(t.Title__c) ? '' : ' ' + t.Title__c)).toUpperCase();
            String last  = (t.LastName__c  == null) ? '' : t.LastName__c;
            String first = (t.FirstName__c == null) ? '' : t.FirstName__c;
            String title = String.isBlank(t.Title__c) ? '' : (' ' + t.Title__c);
                
                // 組字串並轉成大寫
                String pax = (last + '/' + first + title).toUpperCase();
            
            // 最多顯示 21 個字（含空白），多的不要
            if (pax.length() > 19) {
                pax = pax.substring(0, 19); // 取前 18 個字元（0-based，endIndex 不含）
            }
            
            d.paxName = pax;
            
            d.boardingGate = String.valueOf(t.BoardingGate__c);
            d.noGate       = String.isBlank(t.BoardingGate__c);
            
            String timeRaw = String.valueOf(t.LatestBoardingTime__c);
            if (!String.isBlank(timeRaw) && timeRaw.length() == 4) {
                // 取前兩位為小時，後兩位為分鐘
                d.boardingTime = timeRaw.substring(0, 2) + ':' + timeRaw.substring(2, 4);
            } else {
                // 如果不是四碼，直接原樣顯示
                d.boardingTime = timeRaw;
            }

            
            List<String> ms = new List<String>();
            if (!String.isBlank(t.Meal__c))    ms.add(t.Meal__c);
            // if (!String.isBlank(t.Service__c)) ms.add(t.Service__c); 2025-08-29 規格沒有要放特殊服務 Aaron
            d.mealSvc = String.join(ms, '/');
            
            // 附加公告內容進入每一張票（如需要）
            d.noticeContentBoardingPass = noticeContentBoardingPass;
            d.noticeContentEBoardingPass = noticeContentEBoardingPass;
            
            tickets.add(d);
        }
        
        update updateList;
    }
    
    @RemoteAction
    global static String savePdfAttachment(String base64Data, String fileName) {
        if (String.isBlank(base64Data)) return 'No data';

        Blob pdfBlob = EncodingUtil.base64Decode(base64Data);

        Attachment a = new Attachment();
        a.Name = fileName;
        a.Body = pdfBlob;
        a.ContentType = 'application/pdf';
        a.ParentId = CommonController.getAccountIdFromCookie(); // Replace with actual ParentId
        insert a;
        
        return 'Success';
    }
    
