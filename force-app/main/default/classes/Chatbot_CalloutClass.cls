public class Chatbot_CalloutClass {
    public static Time sendOffLineMessage(String language) {
        Map<String,String> MapMSG = new Map<String,String>();
        List<Work_Hour__c> workHours = [SELECT Id, Name, Start_Time__c, End_Time__c, Language__c, OffLineMSG__c, SwitchToBotMSG__c FROM Work_Hour__c WHERE Language__c =: language ORDER BY LastModifiedDate];
        
        List<Temp_Case__c> tempCases = [SELECT Id, Name, Chatbot_UID__c, Chatbot_Language__c, Chatbot_CustomerType__c, Terminated__c FROM Temp_Case__c WHERE CreatedDate = TODAY AND Has_Been_Assigned__c = False AND Chatbot_Language__c =: language];
        List<String> uids = new List<String>();
        List<Temp_Case__c> updateList = new List<Temp_Case__c>();
        for(Temp_Case__c tempCase : tempCases){
            tempCase.Terminated__c = true;
            uids.add(tempCase.Chatbot_UID__c);
            updateList.add(tempCase);
        }
        System.debug(workHours[0].OffLineMSG__c);
        //Send Messege API
        if(uids.size() > 0) Chatbot_CalloutClass.callAPI(uids, workHours[0].OffLineMSG__c);
        if(updateList.size() > 0) update updateList;
        return workHours[0].End_Time__c;
    }
    
    public static void endCalls(List<Case> records) {
        try {
            Map<String,String> MapMSG = new Map<String,String>();
            List<Work_Hour__c> workHours = [SELECT Id, Name, Start_Time__c, End_Time__c, Language__c, OffLineMSG__c, SwitchToBotMSG__c FROM Work_Hour__c ORDER BY LastModifiedDate];
            for(Work_Hour__c workHour : workHours){
                if(workHour.Language__c == 'English') MapMSG.put('English',workHour.SwitchToBotMSG__c);
                if(workHour.Language__c == 'Mandarin') MapMSG.put('Mandarin',workHour.SwitchToBotMSG__c);
                if(workHour.Language__c == 'Japanese') MapMSG.put('Japanese',workHour.SwitchToBotMSG__c);
            }
            
            List<String> EN = new List<String>();
            List<String> TW = new List<String>();
            List<String> JP = new List<String>();
            for(Case record : records){
                //record.Chatbot_UID__c = record.Chatbot_UID__c.replace('-webchat','');
                if(record.Chatbot_Language__c == 'English') EN.add(record.Chatbot_UID__c);
                if(record.Chatbot_Language__c == 'Mandarin') TW.add(record.Chatbot_UID__c);
                if(record.Chatbot_Language__c == 'Japanese') JP.add(record.Chatbot_UID__c);
            }
            
            //Send Messege API
            if(EN.size()>0) Chatbot_CalloutClass.callAPI(EN, MapMSG.get('English'));
            if(TW.size()>0) Chatbot_CalloutClass.callAPI(TW, MapMSG.get('Mandarin'));
            if(JP.size()>0) Chatbot_CalloutClass.callAPI(JP, MapMSG.get('Japanese'));
        } catch(exception e) {
            
        }
    }
    
    @future(callout=true)
    public static void callAPI(List<String> uids, String prompt) {
        Http http = new Http();
        HttpRequest request = new HttpRequest();

        // Set the endpoint
        boolean isSandbox = [SELECT IsSandbox FROM Organization LIMIT 1]?.IsSandbox;
        
        String url = Label.Chatbot_Endpoint + 'api/v4/third_party/starlux/salesforce/case/stop';
        String token = Label.Chatbot_Token;
        /*if(isSandbox) {
            //'https://starlux.ap-mic.com/api/v4/third_party/starlux/salesforce/case/stop'
            url = 'https://chatbot-uat.starlux-airlines.com/api/v4/third_party/starlux/salesforce/case/stop';
            token = 'R2O449xPfLbJ9T0OJpBZbuMHVnK2OZXso6jtY1I1MhMbm0Hx8WucBe7sDbXZGztgg6itaUyWRS7hSLP3VvqDCR20U51mEPXRCtpn22WxGrKJKMJam54uWFF3fbRO9eXt';
        } else {
            url = 'https://chatbot.starlux-airlines.com/api/v4/third_party/starlux/salesforce/case/stop';
            token = '';
        }*/
        request.setEndpoint(url);        
        
        // Set the method
        request.setMethod('POST');
        System.debug(uids);
        Map<String, Object> body = new Map<String, Object>();
        body.put('token', token);
        body.put('uids', uids);
        body.put('prompt', prompt);
        
        // Convert the body to JSON string
        String jsonBody = JSON.serialize(body);
        
        // Set the body
        request.setHeader('Content-Type', 'application/json');
        request.setBody(jsonBody);
        // Send the request
        HttpResponse response = http.send(request);
        
        // Check the response status and handle accordingly
        if (response.getStatusCode() == 200) {
            System.debug('Callout successful: ' + response.getBody());
        } else {
            System.debug('Callout failed: ' + response.getStatus() + ' ' + response.getStatusCode());
        }
    }
    
