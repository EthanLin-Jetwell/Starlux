    public static String normalizefn(String raw) {
        if (String.isBlank(raw)) return null;
        String t = raw.trim().toUpperCase().replaceAll('[^0-9A-Z]', '');
        Matcher m = Pattern.compile('^([0-9]+)([A-Z]?)$').matcher(t);
        if (!m.matches()) return null;
    
        Integer n;
        try { n = Integer.valueOf(m.group(1)); }
        catch (Exception e) { return null; }
    
        String core = String.valueOf(n);
        String suffix = m.group(2);
        return String.isBlank(suffix) ? core : core + suffix;
    }

