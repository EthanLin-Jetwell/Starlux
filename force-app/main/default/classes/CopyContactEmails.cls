    @InvocableMethod(label='Query Cases' description='Queries all related Case records')
    public static List<List<Case>> queryCases(List<Id> contactIdList) {
        //Initialize empty list to store Case records for return
        List<Case> allCases = new List<Case>();
        List<List<Case>> finalCases = new List<List<Case>>();
        
        //Query for records
        String contactId = contactIdList[0];
        allCases = [SELECT Id, Travel_Agent_Alternate_Email_1__c, Travel_Agent_Alternate_Email_2__c, Travel_Agent_Alternate_Email_3__c
                   FROM Case WHERE ContactId = :contactId];
        finalCases.add(allCases);
        
        return finalCases;
    }
