    @InvocableMethod(label='Query Seat Allocation' description='Queries all related Seat Allocation and Ticketing records')
    public static List<List<Seat_Allocation_and_Ticketing__c>> querySeats(List<List<Id>> caseIdList) {
        //Initialize empty list to store Seat Allocation records for return
        List<Seat_Allocation_and_Ticketing__c> allSeats = new List<Seat_Allocation_and_Ticketing__c>();
        List<List<Seat_Allocation_and_Ticketing__c>> finalSeats = new List<List<Seat_Allocation_and_Ticketing__c>>();
        
        //Query for records
        List<Id> caseId = caseIdList[0];
        allSeats = [SELECT Id, Travel_Agent_Alternate_Email_1__c, Travel_Agent_Alternate_Email_2__c, Travel_Agent_Alternate_Email_3__c
                   FROM Seat_Allocation_and_Ticketing__c WHERE Case_ID__c IN :caseId];
        finalSeats.add(allSeats);
        
        return finalSeats;
    }
