    @InvocableMethod(label='Query Seat Sales' description='Queries all related Seat Sales and Tracking records')
    public static List<List<Seat_Sales_Tracking__c>> querySeats(List<List<Id>> caseIdList) {
        //Initialize empty list to store Seat Sales records for return
        List<Seat_Sales_Tracking__c> allSeats = new List<Seat_Sales_Tracking__c>();
        List<List<Seat_Sales_Tracking__c>> finalSeats = new List<List<Seat_Sales_Tracking__c>>();
        
        //Query for records
        List<Id> caseId = caseIdList[0];
        allSeats = [SELECT Id, Travel_Agent_Alternate_Email_1__c, Travel_Agent_Alternate_Email_2__c, Travel_Agent_Alternate_Email_3__c
                   FROM Seat_Sales_Tracking__c WHERE Case_ID__c IN :caseId];
        finalSeats.add(allSeats);
        
        return finalSeats;
    }
