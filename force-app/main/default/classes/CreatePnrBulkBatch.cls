public class CreatePnrBulkBatch implements Database.Batchable<SObject>, Database.AllowsCallouts {
    public Database.QueryLocator start(Database.BatchableContext bc) { 
        Id groupBookingId = [SELECT Id FROM RecordType WHERE DeveloperName = 'Group_Booking' and SObjectType = 'Case'].Id;
        String query = 'SELECT Id FROM Case WHERE Batch_Processing__c = true AND Integration_Link_Status__c = false AND recordTypeId = \'' + groupBookingId + '\'';
        return Database.getQueryLocator(query);
    }

    public void execute(Database.BatchableContext bc, List<SObject> scope) {
        
        List<Seat_Allocation_and_Ticketing__c> satToProcess = new List<Seat_Allocation_and_Ticketing__c>();
        List<Id> caseIdList = new List<Id>();
        for (Integer i = 0; i < scope.size(); i++) {
            if (i >= 100) {
                break;
            }
            Id caseId = (Id) scope.get(i).get('Id');
            caseIdList.add(caseId);
        }
        
        satToProcess = DbUtil.queryAllFieldsWithWhereFieldInList('Seat_Allocation_and_Ticketing__c', 'Case_ID__c', caseIdList);
        
        System.debug('[4i] Batch SAT to process:' + satToProcess.size());
        SeatAllocationUtil.processSatInsertion(satToProcess, Constants.SAT_PROCESS_INSERT_ORIGIN_BATCH);
    }

    public void finish(Database.BatchableContext bc) { 
    }

