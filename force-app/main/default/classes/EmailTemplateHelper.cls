public class EmailTemplateHelper {
    public class RenderedEmail {
        public String subject;
        public String htmlBody;
        public String textBody;

        public RenderedEmail(String subject, String htmlBody, String textBody) {
            this.subject = subject;
            this.htmlBody = htmlBody;
            this.textBody = textBody;
        }
    }

    public static RenderedEmail renderEmailTemplate(Id templateId, Id whatId) {
        Id realContactId = [
            SELECT Id FROM Contact WHERE Email != null LIMIT 1
        ].Id;
        
        Messaging.reserveSingleEmailCapacity(1);

        Messaging.SingleEmailMessage mail = new Messaging.SingleEmailMessage();
        mail.setTargetObjectId(realContactId); // Must be a Contact, Lead, or User
        mail.setTemplateId(templateId);
        mail.setWhatId(whatId);
        mail.setBccSender(false);
        mail.setUseSignature(false);
        mail.setSaveAsActivity(false);

        Savepoint sp = Database.setSavepoint();
        Messaging.sendEmail(new Messaging.SingleEmailMessage[] { mail });
        Database.rollback(sp);  // Prevent sending the actual email
        Database.releaseSavepoint(sp);

        return new RenderedEmail(
            mail.getSubject(),
            mail.getHTMLBody(),
            mail.getPlainTextBody()
        );
    }
    
    public static String renderTable(String htmlTemplate, List<String> paperIds, List<String> digitalIds) {
        Boolean canDigital = !(digitalIds == null || digitalIds.isEmpty());
        Boolean canPaper = !(paperIds == null || paperIds.isEmpty());
        
        Map<Id, TravellerInfo__c> paperInfo = new Map<Id, TravellerInfo__c>([SELECT Id, LastName__c, FirstName__c, Title__c FROM TravellerInfo__c WHERE Id IN :paperIds]);
        Map<Id, TravellerInfo__c> digitalInfo = new Map<Id, TravellerInfo__c>([SELECT Id, LastName__c, FirstName__c, Title__c FROM TravellerInfo__c WHERE Id IN :digitalIds]);
        
        String sfdcBaseURL = URL.getOrgDomainURL().toExternalForm();
        sfdcBaseURL = sfdcBaseURL.replace('salesforce', 'salesforce-sites');
        
        String digitalUrl = sfdcBaseURL+'/apex/EBoardingPassPDF?issend=false&ids=';
        String paperUrl = sfdcBaseURL+'/apex/BoardingPassPrintPDF?issend=false&ids=';
        //String digitalUrlZip = 'https://starlux--starluxtrn.sandbox.my.salesforce-sites.com/apex/EBoardingPassPDF?issend=false&mode=zip&ids=';
        //String paperUrlZip = 'https://starlux--starluxtrn.sandbox.my.salesforce-sites.com/apex/BoardingPassPrintPDF?issend=false&mode=zip&ids=';
        
        //20251016 Richard 建議可以帶入pnr, date等資訊到連結，檔名才能正確顯示
        String digitalUrlZip = sfdcBaseURL+'/apex/EmailBoardingpasses?issend=false&mode=zip&target=e&ids=';
        String paperUrlZip   = sfdcBaseURL+'/apex/EmailBoardingpasses?issend=false&mode=zip&target=p&ids=';

        
        
        String buttonStyle = 'background-color:#8e7248;color:#fff;padding:8px 22px;border:none;border-radius:6px;font-size:16px;cursor:pointer;';
        
        String table = 
            '<div style="max-width:900px;margin:32px auto;">' +
            '<table style="width:100%; border-collapse:collapse; font-family:inherit;">' +
            '<thead>' +
            '<tr>' +
            '<th rowspan="2" style="border:1px solid #e7e2d8; background:#f9f6f2; color:#8e7248; padding:10px; text-align:center; min-width:110px;">旅客姓名</th>';
        
            if (canDigital) table += '<th colspan="2" style="border:1px solid #e7e2d8; background:#f9f6f2; color:#8e7248; padding:10px; text-align:center;">電子登機證</th>';
            if (canPaper) table += '<th colspan="2" style="border:1px solid #e7e2d8; background:#f9f6f2; color:#8e7248; padding:10px; text-align:center;">紙本登機證</th>';
            table += '</tr>';
            table += '<tr>';
            if (canDigital) table += '<th style="border:1px solid #e7e2d8; background:#f9f6f2; color:#8e7248; padding:10px; text-align:center;">PDF(適用Android裝置/桌電)</th>' +
            '<th style="border:1px solid #e7e2d8; background:#f9f6f2; color:#8e7248; padding:10px; text-align:center;">ZIP(適用IOS裝置/桌電)</th>';
            if (canPaper) table += '<th style="border:1px solid #e7e2d8; background:#f9f6f2; color:#8e7248; padding:10px; text-align:center;">PDF(適用Android裝置/桌電)</th>' +
            '<th style="border:1px solid #e7e2d8; background:#f9f6f2; color:#8e7248; padding:10px; text-align:center;">ZIP(適用IOS裝置/桌電)</th>';
            table += '</tr>'+
            '</thead>' +
            '<tbody>';
        
        // --- Row for "All Passengers"
        table +=
            '<tr>' +
            '<td style="border:1px solid #e7e2d8; padding:12px; text-align:center;">全部旅客</td>';
            if (canDigital) table += '<td style="border:1px solid #e7e2d8; padding:12px; text-align:center;"><a href="' + digitalUrl + String.join(digitalIds, ',') + '" target="_blank" style="' + buttonStyle + '">下載</a></td>' +
            '<td style="border:1px solid #e7e2d8; padding:12px; text-align:center;"><a href="' + digitalUrlZip + String.join(digitalIds, ',') + '" target="_blank" style="' + buttonStyle + '" download="boarding-passes.zip">下載</a></td>';
            if (canPaper) table += '<td style="border:1px solid #e7e2d8; padding:12px; text-align:center;"><a href="' + paperUrl + String.join(paperIds, ',') + '" target="_blank" style="' + buttonStyle + '">下載</a></td>' +
            '<td style="border:1px solid #e7e2d8; padding:12px; text-align:center;"><a href="' + paperUrlZip + String.join(paperIds, ',') + '" target="_blank" style="' + buttonStyle + '" download="boarding-passes.zip">下載</a></td>';
            table += '</tr>';
        
        // --- Rows for individual passengers
        List<String> ids;
        if (!(paperIds == null && paperIds.isEmpty())) ids = paperIds;
        else if (!(digitalIds == null && digitalIds.isEmpty())) ids = digitalIds;
        else ids = new List<String>();
        for (String pId : ids) {
            String firstName = paperInfo.get(pId).FirstName__c;
            String lastName = paperInfo.get(pId).LastName__c;
            
            if (!(paperIds == null && paperIds.isEmpty())) {
                firstName = paperInfo.get(pId).FirstName__c;
                lastName = paperInfo.get(pId).LastName__c;
            } else if (!(digitalIds == null && digitalIds.isEmpty())) {
                firstName = digitalInfo.get(pId).FirstName__c;
                lastName = digitalInfo.get(pId).LastName__c;
            }
            
            String digitalPdf = digitalUrl + pId;
            String digitalZip = digitalUrlZip + pId;
            String paperPdf = paperUrl + pId;
            String paperZip = paperUrlZip + pId;
            
            table +=
                '<tr>' +
                '<td style="border:1px solid #e7e2d8; padding:12px; text-align:center;">' + firstName + ' ' + lastName + '</td>';
                if (canDigital) table += '<td style="border:1px solid #e7e2d8; padding:12px; text-align:center;"><a href="' + digitalPdf + '" target="_blank" style="' + buttonStyle + '">下載</a></td>' +
                '<td style="border:1px solid #e7e2d8; padding:12px; text-align:center;"><a href="' + digitalZip + '" target="_blank" style="' + buttonStyle + '" download="boarding-passes.zip">下載</a></td>';
                if (canPaper) table += '<td style="border:1px solid #e7e2d8; padding:12px; text-align:center;"><a href="' + paperPdf + '" target="_blank" style="' + buttonStyle + '">下載</a></td>' +
                '<td style="border:1px solid #e7e2d8; padding:12px; text-align:center;"><a href="' + paperZip + '" target="_blank" style="' + buttonStyle + '" download="boarding-passes.zip">下載</a></td>';
                table += '</tr>';
        }
        
        table += '</tbody></table></div>';

        
        htmlTemplate = htmlTemplate
            .replace('{table}', table);
        
        return htmlTemplate;
    }
    
