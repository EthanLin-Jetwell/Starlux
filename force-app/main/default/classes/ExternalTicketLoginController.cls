public class ExternalTicketLoginController {
    public String email {get; set;}
    public String Taxid {get; set;} //上
    public String CompanyType {get; set;} //上
    public String UserAccount {get; set;} //中
    public String DepartmentCode {get; set;} //中
    public String password {get; set;} //下
    public String VerificationCode {get; set;} //下
    
    public String errorMessage {get; set;}
    public Boolean isVisible {get; set;}
    public Integer minutes {get; set;}
    public Boolean isWrongCredentials { get; set; }
    public Boolean isAccountInactive { get; set; }
    public Boolean isPasswordExpired { get; set; }
    public Boolean isPersonIDMismatch = false;
    public String accountId { get; set; }
    public Boolean islogout { get; set; }
    public Integer logoutminutes { get; set; } // 用於存放登出分鐘數
    
    public ExternalTicketLoginController() {
        isVisible = false;
        
        // 檢查 URL 參數並設置 islogout
        String isLogoutParam = ApexPages.currentPage().getParameters().get('islogout');
        if (isLogoutParam == 'true') {
            islogout = true;
        } else {
            islogout = false;
        }
        
        // 初始化分鐘數，並查詢參數值
        logoutminutes = 0;
        try {
            B2B_Parameter__c parameter = [
                SELECT Value__c 
                FROM B2B_Parameter__c 
                WHERE Name = 'P007'
                LIMIT 1
            ];
            
            // 將秒數轉換為分鐘數
            if (parameter != null && parameter.Value__c != null) {
                Integer seconds = Integer.valueOf(parameter.Value__c);
                logoutminutes = seconds / 60; // 秒數轉分鐘
            }
        } catch (Exception e) {
            // 錯誤處理：minutes 保持為 0 或給出預設值
            logoutminutes = 30;
        }
    }
    

    public Pagereference login() {
        // 重置狀態
        isWrongCredentials = false;
        isAccountInactive = false;
        isPasswordExpired = false;
        isPersonIDMismatch = false;
        accountId = null;
        
        // 查詢符合信相和統編的員工資料
        Account acc = ExternalTicketLoginUtility.getAccount(CompanyType,DepartmentCode,VerificationCode);
        system.debug(acc);
        
        // 檢查是否存在此帳號
        if (acc == null) {
            isWrongCredentials = true;
            return null;
        }
        
        // 設置 accountId
        accountId = acc.Id;
        system.debug(accountId);

        // 登入成功
        ApexPages.addMessage(new ApexPages.Message(ApexPages.Severity.CONFIRM, 'Login Successfully.'));
        ExternalTicketLoginUtility.saveAccountLoginCookie(acc);
        PageReference home = Page.ExternalTicketHome;
        home.getParameters().put('toast', 'login');
        home.setRedirect(true); // 交給瀏覽器做 302，會自動採用正確網域
        return home;
    }


    public Pagereference forgotPassword() {
        return PageUtility.toPage(UrlConstants.FORGOT_PASSWORD);
    }
