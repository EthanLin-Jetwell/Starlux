public class HOME_Controller {
    
    public Boolean isPasswordExpiring { get; set; }
    public Date passwordExpirationDate { get; set; }    
    public String UserId { get; set; }
    public String noticeContent { get; set; }  // 新增用於存儲公告內容的屬性
    
    public HOME_Controller() {
        checkPasswordExpiration();
        loadNoticeContent();  // 在構造函數中調用加載公告內容的方法
    }
    
    private void checkPasswordExpiration() {
        String UserId = ApexPages.currentPage().getCookies().get('accountId').getValue();
    	System.debug('userId'+UserId);
        if (String.isNotBlank(UserId)) {
            List<UserManagement__c> UserList = [SELECT Id, ExpireDate__c FROM UserManagement__c WHERE Id = :UserId LIMIT 1];
    
            if (!UserList.isEmpty()) {
                UserManagement__c User = UserList[0];
                System.debug('user:'+User);
                Date expirationDate = User.ExpireDate__c;
                Date today = Date.today();
    			System.debug('expirationDate:'+expirationDate);
                System.debug('today:'+today);
                
                if (expirationDate != null) {
                    Integer daysThreshold = 3; // 預設值
                    try {
                        B2B_Parameter__c parameter = [SELECT Value__c FROM B2B_Parameter__c WHERE Name = 'P004' LIMIT 1];
                        if (parameter != null && String.isNotBlank(parameter.Value__c)) {
                            daysThreshold = Integer.valueOf(parameter.Value__c);
                        }
                        System.debug('daysThreshold:'+daysThreshold);
                    } catch (Exception e) {
                        system.debug('無法取得參數 P004：' + e.getMessage());
                    }
    
                    Integer daysDifference = today.daysBetween(expirationDate);
    				System.debug('daysDifference:'+daysDifference);
                    if (daysDifference <= daysThreshold && daysDifference >= 0) {
                        isPasswordExpiring = true;
                        passwordExpirationDate = expirationDate;
                        System.debug('快過期了');
                    } else {
                        isPasswordExpiring = false;
                        System.debug('還沒過期');
                    }
                }
            }
        }
    }

    
    // 新增加載公告內容的方法
    private void loadNoticeContent() {
        try {
            List<NoticeSetting__c> notices = [SELECT Content__c FROM NoticeSetting__c where Name = '首頁' LIMIT 1];
            if (!notices.isEmpty() && notices[0].Content__c != null) {
                noticeContent = notices[0].Content__c;
            } else {
                noticeContent = ''; // 如果沒有公告內容，設置為空字符串
            }
        } catch (Exception e) {
            noticeContent = ''; // 發生錯誤時設置為空字符串
            System.debug('Error loading notice content: ' + e.getMessage());
        }
    }
    
    // 提供一個公開的getter方法來訪問公告內容
    public String getNoticeContent() {
        return noticeContent;
    }
    
