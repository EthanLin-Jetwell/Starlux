    // 新增獲取起飛地清單的方法
    @RemoteAction
    public static List<String> getDepartureList() {
        List<String> departureList = new List<String>();
        
        // 使用 DISTINCT 關鍵字來避免重複值
        
        for(AggregateResult ar : [
            SELECT Departure__c 
            FROM AirportCode__c 
            WHERE RecordType.name = 'B2B Airport Code'
            AND   Departure__c != null 
            AND   Active__c = true
            GROUP BY Departure__c 
            ORDER BY Departure__c ASC
        ]) {
            departureList.add((String)ar.get('Departure__c'));
        }
        
        return departureList;
    }

    

    @RemoteAction
    public static List<OperationHistoryWrapper> searchRecords(String flightNumber, String departureDate, String departure, String bookingCode, String operationType) {
        String userId = UserInfo.getUserId();
        Boolean isAdminProfile = [SELECT Profile.PermissionsModifyAllData 
                                  FROM User 
                                  WHERE Id = :userId].Profile.PermissionsModifyAllData;
        // Check if the user has any permission set that grants "Modify All Data"
        /*Boolean isAdminViaPermissionSet = [
            SELECT PermissionsModifyAllData 
            FROM PermissionSetAssignment 
            WHERE AssigneeId = :userId
            AND PermissionSet.PermissionsModifyAllData = true
            LIMIT 1
        ] != null;*/
        
        // 建立動態查詢條件
        String query = 'SELECT OperationHistory__r.User_Management_Temp__r.CN_Passport__c, ' +
            'OperationHistory__r.FlightNumber__c, ' +
            'OperationHistory__r.DepartureDate__c, ' +
            'OperationHistory__r.Departure__c, ' +
            'Id, IsDeleted, Name, ' +
            'OperationHistory__c, ' +  // 新增這個欄位
            'OperationHistory__r.CreatedDate, ' +
            'OperationHistory__r.Operation__c, ' +
            'OperationHistory__r.Menu__c, ' +
            'Value__c, CustomerName__c ' +
            'FROM OperationHistoryDetail__c ' +
            'WHERE IsDeleted = FALSE ';
        
        // 加入搜尋條件
        if(String.isNotBlank(flightNumber)) {
            query += ' AND OperationHistory__r.FlightNumber__c = :flightNumber';
        }
        if(String.isNotBlank(departureDate)) {
            Date searchDate = Date.valueOf(departureDate);
            query += ' AND OperationHistory__r.DepartureDate__c = :searchDate';
        }
        /*無法搜尋 2025/8/20 Aaron
        if(String.isNotBlank(bookingCode)) {
            query += ' AND OperationHistory__r.PNR__c LIKE : (\'%\' + bookingCode + \'%\')';
        }
		*/
        if(String.isNotBlank(bookingCode)) {
    		bookingCode = '%' + bookingCode + '%'; // 在Apex端處理好
 		   query += ' AND OperationHistory__r.PNR__c LIKE :bookingCode';
		}

		
        if(String.isNotBlank(departure)) {
            query += ' AND OperationHistory__r.Departure__c = :departure';
        }
        if(String.isNotBlank(operationType) && operationType != '請選擇') {
            query += ' AND OperationHistory__r.Menu__c = :operationType';
        }
        if(!isAdminProfile) {
            //query += ' AND OwnerId = :userId';
        }
        
        query += ' ORDER BY OperationHistory__r.CreatedDate DESC, CustomerName__c ASC';
        System.debug(flightNumber);
        System.debug(bookingCode);
        System.debug(departure);
        System.debug(operationType);
        
        // 執行查詢並組織資料
        Map<Id, OperationHistoryWrapper> historyMap = new Map<Id, OperationHistoryWrapper>();
        
        for(OperationHistoryDetail__c detail : Database.query(query)) {
            Id historyId = detail.OperationHistory__c;
            
            if(!historyMap.containsKey(historyId)) {
                OperationHistoryWrapper hw = new OperationHistoryWrapper();
                hw.Id = historyId;
                hw.UserPassport = detail.OperationHistory__r.User_Management_Temp__r.CN_Passport__c;
                hw.Menu = detail.OperationHistory__r.Menu__c;
                hw.Operation = detail.OperationHistory__r.Operation__c;
                hw.CreatedDate = detail.OperationHistory__r.CreatedDate;
                historyMap.put(historyId, hw);
            }
            
            DetailWrapper dw = new DetailWrapper();
            dw.Id = detail.Id;
            dw.CustomerName = detail.CustomerName__c;
            dw.Value = processHtmlContent(detail.Value__c);
            
            historyMap.get(historyId).Details.add(dw);
        }
        System.debug(historyMap);
        return historyMap.values();
    }
    
    private static String processHtmlContent(String htmlContent) {
        if (String.isBlank(htmlContent)) return '';
        
        String processedContent = htmlContent
            .replace('\\', '')
            .replace('\\"', '"')
            .replace('\\n', '\n')
            .replace('\\r', '\r');
        
        return processedContent;
    }
    
