    public String  flightNumber  { get; set; }
    public String IATAcode { get; set; }
    
    // Detail 包裝類
    public class DetailWrapper {
        public String Id { get; set; }
        public String CustomerName { get; set; }
        public String Value { get; set; }
    }
    
    // 主要的包裝類，用於顯示 Operation History
    public class OperationHistoryWrapper {
        public String Id { get; set; }
        public String UserPassport { get; set; }
        public String Menu { get; set; }
        public String Operation { get; set; }
        public DateTime CreatedDate { get; set; }
        public List<DetailWrapper> Details { get; set; }
        
        public OperationHistoryWrapper() {
            Details = new List<DetailWrapper>();
        }
    }
    
    // 包裝類，用於處理查詢結果
    public class OperationHistoryDetailWrapper {
        public String Id { get; set; }
        public String UserPassport { get; set; }
        public String Menu { get; set; }
        public String Operation { get; set; }
        public String CustomerName { get; set; }
        public String AllValue { get; set; }
        public DateTime CreatedDate { get; set; }
        public DateTime AdjustedCreatedDate { get; set; } // 存儲調整後的 CreatedDate (加 8 小時)
        
        // 包裝類的構造函數
        public OperationHistoryDetailWrapper(OperationHistoryDetail__c detail) {
            this.Id = detail.Id;
            this.UserPassport = detail.OperationHistory__r.User_Management_Temp__r.CN_Passport__c;
            this.Menu = detail.OperationHistory__r.Menu__c;
            this.Operation = detail.OperationHistory__r.Operation__c;
            this.CustomerName = detail.CustomerName__c;
            this.CreatedDate = detail.OperationHistory__r.CreatedDate;
            
            // 基本的 HTML 實體預處理
            if (detail.Value__c != null) {
                this.AllValue = detail.Value__c
                    .replace('\\', '')  // 移除多餘的反斜線
                    .replace('\\"', '"')  // 處理引號
                    .replace('\\n', '\n')  // 處理換行符
                    .replace('\\r', '\r');  // 處理回車符
            }
            
            this.AdjustedCreatedDate = this.CreatedDate.addHours(8);
        }
    }

