    private static Id createEmailLog(String subject, Id userManagementId) {
        if(userManagementId != null) {
            EmailLog__c emailLog = new EmailLog__c(Name=subject, User__c=userManagementId);
            insert emailLog;
            
            String sfdcBaseURL = URL.getOrgDomainURL().toExternalForm();
            sfdcBaseURL = sfdcBaseURL.replace('salesforce', 'salesforce-sites');
            
            update new UserManagement__c(Id=userManagementId, Resend__c=false, Link__c=sfdcBaseURL+'/apex/ResetPassword?emailLogId='+emailLog.Id);
            
            return emailLog.Id;
        }
        return null;
    }
    
    public static void updateEmailLog(Id logId, String subject, String email, String link, String response, String body, String status, Integer statusCode) {
        switch on String.valueOf(logId.getSObjectType()) {
            when 'EmailLog__c'{
                Integer addTime = Integer.valueOf([SELECT Value__c FROM B2B_Parameter__c WHERE ParameterCode__c = 'RegisterExpire' LIMIT 1]?.Value__c);
                DateTime expireDate = System.now().addSeconds(addTime);
                EmailLog__c emailLog = new EmailLog__c(Id=logId, Name='Email Log', Subject__c=subject, Receiver__c=email, Link__c=link, Response__c=response.left(3000), Body__c=body.left(3000), Status__c=status, Status_Code__c=statusCode, Expire_Date__c=expireDate);
                update emailLog;
            }
            when 'CGD_Announcement_Target__c'{
                CGD_Announcement_Target__c emailLog = new CGD_Announcement_Target__c(Id=logId, Receiver__c=email, Response__c=response, Status_Code__c=statusCode);
                update emailLog;
            }
        }
        
    }
    
    public static void updateEmailLogs(List<Id> logIds, String response, String body, String status, Integer statusCode) {
        List<CGD_Announcement_Target__c> logs = new List<CGD_Announcement_Target__c>();
        try {
            MessageResponse res = MessageResponse.parse(response);
            
            for (MessageResponse.Message msg : res.messages) {
                logs.add(new CGD_Announcement_Target__c(Id=msg.messageId, Receiver__c=msg.destination, Response__c=response, Status_Code__c=statusCode));
            }
        } catch(exception e) {
            for (Id i : logIds) {
                logs.add(new CGD_Announcement_Target__c(Id=i, Response__c=response, Status_Code__c=statusCode));
            }
        }
        
        update logs;
    }
    
