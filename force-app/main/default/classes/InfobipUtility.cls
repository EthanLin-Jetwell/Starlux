    public static void sendBoardingPassEmail(List<String> recipientEmails, Id userManagementId, BoardingPassStats stats) {
        // Step 1: Get user + latest email log
        Id emailLogId = createEmailLog('Boarding Pass', userManagementId);
        
        EmailTemplate template = [
            SELECT Id
            FROM EmailTemplate
            WHERE Name = 'B2B ç™»æ©Ÿè­‰é€šçŸ¥'
            LIMIT 1
        ];
        
        Id templateId = template.Id;
        
        // ðŸ‘‡ Use helper to render template content
        EmailTemplateHelper.RenderedEmail rendered = EmailTemplateHelper.renderEmailTemplate(
            templateId,
            userManagementId
        );
        
        String subject = rendered.subject;
        String body = rendered.htmlBody;
        
        Map<String, String> variables = stats.toVariableMap();
        
        for (String key : variables.keySet()) {
            body = body.replace('{' + key + '}', variables.get(key));
            subject = subject.replace('{' + key + '}', variables.get(key));
            if(variables.get(key) == '') {
                if (key == 'digitalUrl') {
                    body = body.replaceAll('<a[^>]*\\{digitalUrl\\}[^>]*>.*?</a>', '');
                }
                if (key == 'paperUrl') {
                    body = body.replaceAll('<a[^>]*\\{paperUrl\\}[^>]*>.*?</a>', '');
                }
            }
        }
        
        System.debug(stats.digitalIds);
        
        body = EmailTemplateHelper.renderTable(body, stats.paperIds, stats.digitalIds);
        
        // Step 3: Fetch file attachment (optional)
        List<EmailAttachment> attachments = new List<EmailAttachment>();
        
        List<Id> docIds = new List<id>();
        for (ContentDocumentLink doc : [SELECT ContentDocumentId
                                        FROM ContentDocumentLink
                                        WHERE LinkedEntityId = :templateId]) {
                                            docIds.add(doc.ContentDocumentId);
                                        }
        
        for (ContentVersion con : [SELECT Id, Title, FileType, VersionData, ContentDocumentId
                                   FROM ContentVersion
                                   WHERE ContentDocumentId IN :docIds AND IsLatest = true]) {
                                       String base64Content = EncodingUtil.base64Encode(con.VersionData);
                                       String mimeType = con.FileType;
                                       String fileName = con.Title+'.'+mimeType;
                                       
                                       // If you're using a custom wrapper class, adjust this line accordingly:
                                       attachments.add(new EmailAttachment(fileName, mimeType, base64Content));
                                   }
        
        // Step 4: Send Infobip email using helper
        // Enqueue the job
        System.enqueueJob(new InfobipUtility(recipientEmails,
                                             subject,
                                             body,
                                             emailLogId,
                                             attachments));
    }

    public static void sendInfobipEmail(String senderEmail, List<String> recipientEmails, String subject, String htmlBody, Id logId, List<EmailAttachment> attachments) {
        if (recipientEmails == null || recipientEmails.isEmpty()) {
            //throw new AuraHandledException('Recipient email list cannot be empty.');
        }
        
        if (String.isBlank(senderEmail)) {
            senderEmail = 'noreply@starlux-airlines.com';
        }
        
        List<Object> destinations = new List<Object>();
        for (String email : recipientEmails) {
            destinations.add(new Map<String, Object>{
                'to' => new List<Object>{
                    new Map<String, Object>{ 'destination' => email }
                }
            });
        }
    
        System.debug(attachments);
        // Handle attachments if any
        Object attachmentsField;
        if (attachments != null && !attachments.isEmpty()) {
            List<Object> attachmentList = new List<Object>();
            for (EmailAttachment att : attachments) {
                attachmentList.add(att.toInfobipFormat());
            }
            attachmentsField = attachmentList;
        }
    
        System.debug(htmlBody);
        
        Map<String, Object> content = new Map<String, Object>{
            'subject' => subject,
            'html' => htmlBody
        };
    
        if (attachmentsField != null) {
            content.put('attachments', attachmentsField);
        }
    
        Map<String, Object> message = new Map<String, Object>{
            'sender' => 'Starlux <'+senderEmail+'>',
            'destinations' => destinations,
            'content' => content
        };
    
        Map<String, Object> payload = new Map<String, Object>{
            'messages' => new List<Object>{ message }
        };
    
        String jsonBody = JSON.serialize(payload);
    
        HttpRequest req = new HttpRequest();
        req.setEndpoint('https://e554gr.api.infobip.com/email/4/messages');
        req.setMethod('POST');
        req.setHeader('Authorization', 'App 177ae8fa48fc5a23674f376161a649e1-2abe7a43-91e9-436a-b69a-7d651c034d9b');
        req.setHeader('Content-Type', 'application/json');
        req.setHeader('Accept', 'application/json');
        req.setBody(jsonBody);
    
        Http http = new Http();
        try {
            HttpResponse res = http.send(req);
            updateEmailLog(logId, subject, String.join(recipientEmails, ','), '', res.getBody(), jsonBody, res.getStatus(), res.getStatusCode());
            System.debug('Infobip Email Sent - Status: ' + res.getStatusCode());
            
            System.debug('Infobip Response: ' + res.getBody());
        } catch (Exception ex) {
            updateEmailLog(logId, subject, String.join(recipientEmails, ','), '', ex.getMessage(), jsonBody, 'apex error', -1);
            System.debug('Error sending Infobip email: ' + ex.getMessage());
        }
    }
        
    public static void sendInfobipEmails(String senderEmail, List<String> recipientEmails, String subject, String htmlBody, List<Id> logIds, List<EmailAttachment> attachments) {
        if (recipientEmails == null || recipientEmails.isEmpty()) {
            //throw new AuraHandledException('Recipient email list cannot be empty.');
        }
        
        if (String.isBlank(senderEmail)) {
            senderEmail = 'noreply@starlux-airlines.com';
        }
        
        List<Object> destinations = new List<Object>();
        Integer i = 0;
        for (String email : recipientEmails) {
            destinations.add(new Map<String, Object>{
                'to' => new List<Object>{
                    new Map<String, Object>{ 'destination' => email, 'messageId' => logIds[i] }
                }
            });
            
            i++;
        }
    
        // Handle attachments if any
        Object attachmentsField;
        if (attachments != null && !attachments.isEmpty()) {
            List<Object> attachmentList = new List<Object>();
            for (EmailAttachment att : attachments) {
                attachmentList.add(att.toInfobipFormat());
            }
            attachmentsField = attachmentList;
        }
    
        System.debug(htmlBody);
        
        Map<String, Object> content = new Map<String, Object>{
            'subject' => subject,
            'html' => htmlBody
        };
    
        if (attachmentsField != null) {
            content.put('attachments', attachmentsField);
        }
    
        Map<String, Object> message = new Map<String, Object>{
            'sender' => 'Starlux <'+senderEmail+'>',
            'destinations' => destinations,
            'content' => content
        };
    
        Map<String, Object> payload = new Map<String, Object>{
            'messages' => new List<Object>{ message }
        };
    
        String jsonBody = JSON.serialize(payload);
    
        HttpRequest req = new HttpRequest();
        req.setEndpoint('https://e554gr.api.infobip.com/email/4/messages');
        req.setMethod('POST');
        req.setHeader('Authorization', 'App 177ae8fa48fc5a23674f376161a649e1-2abe7a43-91e9-436a-b69a-7d651c034d9b');
        req.setHeader('Content-Type', 'application/json');
        req.setHeader('Accept', 'application/json');
        req.setBody(jsonBody);
    
        Http http = new Http();
        try {
            HttpResponse res = http.send(req);
            updateEmailLogs(logIds, res.getBody(), jsonBody, res.getStatus(), res.getStatusCode());
            System.debug('Infobip Email Sent - Status: ' + res.getStatusCode());
            
            System.debug('Infobip Response: ' + res.getBody());
        } catch (Exception ex) {
            updateEmailLogs(logIds, ex.getMessage(), jsonBody, 'apex error', -1);
            System.debug('Error sending Infobip email: ' + ex.getMessage());
        }
    }
 
