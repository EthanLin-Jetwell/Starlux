public class LoginController {
    public String email {get; set;}
    public String IATAcode {get; set;}
    public String password {get; set;}
    public String UserAccount {get; set;}
    public String errorMessage {get; set;}
    public Boolean isVisible {get; set;}
    public Integer minutes {get; set;}
    public Boolean isWrongCredentials { get; set; }
    public Boolean isAccountInactive { get; set; }
    public Boolean isPasswordExpired { get; set; }
    public Boolean isPersonIDMismatch = false;
    public String accountId { get; set; }
    public Boolean islogout { get; set; }
    public Integer logoutminutes { get; set; } // 用於存放登出分鐘數
    
    public LoginController() {
        isVisible = false;
        
        // 檢查 URL 參數並設置 islogout
        String isLogoutParam = ApexPages.currentPage().getParameters().get('islogout');
        if (isLogoutParam == 'true') {
            islogout = true;
        } else {
            islogout = false;
        }
        
        // 初始化分鐘數，並查詢參數值
        logoutminutes = 0;
        try {
            B2B_Parameter__c parameter = [
                SELECT Value__c 
                FROM B2B_Parameter__c 
                WHERE Name = 'P007'
                LIMIT 1
            ];
            
            // 將秒數轉換為分鐘數
            if (parameter != null && parameter.Value__c != null) {
                Integer seconds = Integer.valueOf(parameter.Value__c);
                logoutminutes = seconds / 60; // 秒數轉分鐘
            }
        } catch (Exception e) {
            // 錯誤處理：minutes 保持為 0 或給出預設值
            logoutminutes = 30;
        }
    }
    

    public Pagereference login() {
        // 重置狀態
        isWrongCredentials = false;
        isAccountInactive = false;
        isPasswordExpired = false;
        isPersonIDMismatch = false;
        accountId = null;
        
        // 查詢符合信相和統編的員工資料
        
        UserManagement__c acc = UserUtilities.getUserByAccountAndIATAcode(UserAccount, IATAcode);
        
        system.debug(acc);
        
        // 檢查是否存在此帳號
        if (acc == null) {
            isWrongCredentials = true;
            return null;
        }
        UserLoginHistory__c loginHistory = new UserLoginHistory__c();
        loginHistory.UserManagement__c = acc.Id;
        loginHistory.UserManagementTemp__c = acc.UserManagementTemp__c;
        
        // 設置 accountId
        accountId = acc.Id;
        
        system.debug(accountId);

        // 檢查帳號是否開通
        if (!acc.Verify__c) {
            isAccountInactive = true;
            acc.Resend__c = true; 
            update acc;
            loginHistory.Status__c = '尚未開通';
            Insert loginHistory;
            return null;
        }
        
        // 檢查密碼是否過期
        if (LoginUtility.isPasswordExpired(acc)) {
            isPasswordExpired = true;
            loginHistory.Status__c = '密碼逾期';
            Insert loginHistory;
            return null;
        }
        
        // 驗證帳號及密碼
        if (!LoginUtility.verifyAccountLogin(acc, UserAccount, password)) {
            isWrongCredentials = true; // 帳號存在但密碼錯誤
            loginHistory.Status__c = '密碼錯誤';
            Insert loginHistory;
            return null;
        }

        // 登入成功
        ApexPages.addMessage(new ApexPages.Message(ApexPages.Severity.CONFIRM, '登入成功！'));
        loginHistory.Status__c = '登入成功';
        Insert loginHistory;
        LoginUtility.saveLoginCookie(accountId);
        
        String homeUrl = UrlConstants.HOME + '?accountId=' + EncodingUtil.urlEncode(accountId, 'UTF-8');
        return PageUtility.toPage(homeUrl);
    }


    public Pagereference forgotPassword() {
        return PageUtility.toPage(UrlConstants.FORGOT_PASSWORD);
    }
    
