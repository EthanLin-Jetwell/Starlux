public class LoginHistoryController {
    // 用於存放處理後的資料
    public List<OperationHistoryDetailWrapper> operationHistoryDetails { get; set; }
    public String  flightPrefix  { get; set; }
    public String  flightNumber  { get; set; }
    public String IATAcode { get; set; }
    public String Admin { get; set; }
    public String CurrentUserName    { get; set; }
    public String CurrentUserCompany { get; set; }
    
    // Detail 包裝類
    public class DetailWrapper {
        public String Id { get; set; }
        public String CustomerName { get; set; }
        public String Value { get; set; }
    }
    
    
    // === 登入紀錄用的包裝類（前端用 currentResults 接收）===
    public class LoginHistoryResultWrapper {
        public String Id { get; set; }                    // UserLoginHistory__c.Id
        public String UserManagementTempId { get; set; }  // 編號：UserLoginHistory__c.UserManagementTemp__c
        public String Account { get; set; }               // 帳號：UserLoginHistory__c.UserManagementTemp__r.Account__c
        public String Company { get; set; }               // 隸屬公司：UserLoginHistory__c.UserManagementTemp__r.Company__c
        public String Status { get; set; }                // 狀態：UserLoginHistory__c.Status__c
        public Datetime CreatedDate { get; set; }         // 建立日期：UserLoginHistory__c.CreatedDate
        public String CreatedDateGmtStr { get; set; }     // 給前端顯示用的 GMT+0 字串
    }
    
    // 主要的包裝類，用於顯示 Operation History
    public class OperationHistoryWrapper {
        public String Id { get; set; }
        public String UserPassport { get; set; }
        public String Menu { get; set; }
        public String Operation { get; set; }
        public DateTime CreatedDate { get; set; }
        public List<DetailWrapper> Details { get; set; }
        
        public OperationHistoryWrapper() {
            Details = new List<DetailWrapper>();
        }
    }
    
    // 包裝類，用於處理查詢結果
    public class OperationHistoryDetailWrapper {
        public String Id { get; set; }
        public String UserPassport { get; set; }
        public String Menu { get; set; }
        public String Operation { get; set; }
        public String CustomerName { get; set; }
        public String AllValue { get; set; }
        public DateTime CreatedDate { get; set; }
        public DateTime AdjustedCreatedDate { get; set; } // 存儲調整後的 CreatedDate (加 8 小時)
        
        // 包裝類的構造函數
        public OperationHistoryDetailWrapper(OperationHistoryDetail__c detail) {
            this.Id = detail.Id;
            this.UserPassport = detail.OperationHistory__r.User_Management_Temp__r.CN_Passport__c;
            this.Menu = detail.OperationHistory__r.Menu__c;
            this.Operation = detail.OperationHistory__r.Operation__c;
            this.CustomerName = detail.CustomerName__c;
            this.CreatedDate = detail.OperationHistory__r.CreatedDate;
            
            // 基本的 HTML 實體預處理
            if (detail.Value__c != null) {
                this.AllValue = detail.Value__c
                    .replace('\\', '')  // 移除多餘的反斜線
                    .replace('\\"', '"')  // 處理引號
                    .replace('\\n', '\n')  // 處理換行符
                    .replace('\\r', '\r');  // 處理回車符
            }
            
            this.AdjustedCreatedDate = this.CreatedDate.addHours(8);
        }
    }

    // 初始化方法
    public LoginHistoryController() {
        flightPrefix = 'JX';
        Map<String, Cookie> cookies = ApexPages.currentPage().getCookies();
        if (cookies != null && cookies.containsKey('accountId')) {
            try {
                UserManagement__c usr = [
                    SELECT Id, IATA_MSO_Code__c, Role__c, Name, Account__c, Company__r.Name
                    FROM UserManagement__c
                    WHERE Id = :cookies.get('accountId').getValue()
                    LIMIT 1
                ];
                IATAcode = usr.IATA_MSO_Code__c;
                //0=admin,1=user
                Admin = usr.Role__c;
                CurrentUserName    = usr.Name;
            	CurrentUserCompany = usr.Company__r.Name;
                System.debug('使用者 IATA Code = ' + IATAcode);
            } catch (Exception e) {
                System.debug('查詢 UserManagement__c 失敗：' + e.getMessage());
                IATAcode           = null;
                Admin              = null;
                CurrentUserName    = null;
                CurrentUserCompany = null;
            }
        }
        
        operationHistoryDetails = new List<OperationHistoryDetailWrapper>();
    }
    
    // 1. 取得 Status 清單
    @RemoteAction
    public static List<String> getStatusList() {
        List<String> result = new List<String>();
        for (AggregateResult ar : [
            SELECT Status__c status
            FROM UserLoginHistory__c
            WHERE Status__c != null
            GROUP BY Status__c
            ORDER BY Status__c ASC
        ]) {
            result.add((String)ar.get('status'));
        }
        return result;
    }

    // 2. 取得 UserManagementTemp__r.Name 清單
    @RemoteAction
    public static List<String> getUserManagementTempList() {
        List<String> result = new List<String>();
        for (AggregateResult ar : [
            SELECT UserManagementTemp__r.Name tempName
            FROM UserLoginHistory__c
            WHERE UserManagementTemp__c != null
            GROUP BY UserManagementTemp__r.Name
            ORDER BY UserManagementTemp__r.Name ASC
        ]) {
            result.add((String)ar.get('tempName'));
        }
        return result;
    }

    // 3. 取得 UserManagementTemp__r.Company__c 清單
    @RemoteAction
    public static List<String> getCompanyList() {
        List<String> result = new List<String>();
        for (AggregateResult ar : [
            SELECT UserManagementTemp__r.Company__c company
            FROM UserLoginHistory__c
            WHERE UserManagementTemp__r.Company__c != null
            GROUP BY UserManagementTemp__r.Company__c
            ORDER BY UserManagementTemp__r.Company__c ASC
        ]) {
            result.add((String)ar.get('company'));
        }
        return result;
    }

    // === 新版搜尋：查 UserLoginHistory__c ===
    @RemoteAction
    public static List<LoginHistoryResultWrapper> searchRecords(
        String loginDateStr,   // yyyy-MM-dd
        String status,         // Status__c
        String userTempId,     // UserManagementTemp__c
        String company         // UserManagementTemp__r.Company__c
    ) {
        List<LoginHistoryResultWrapper> results = new List<LoginHistoryResultWrapper>();
        
        System.debug('選取日期'+loginDateStr);
        
        // 1. 登入日期必填：前端沒給就直接空結果
        if (String.isBlank(loginDateStr)) {
            return results;
        }
        
        Date loginDate;
        try {
            loginDate = Date.valueOf(loginDateStr);
        } catch (Exception e) {
            // 日期格式錯誤，直接回空
            return results;
        }
        
        // 2. 用「當天 00:00:00 ～ 隔天 00:00:00」當區間查 CreatedDate
        Datetime startDt = Datetime.newInstance(loginDate.year(), loginDate.month(), loginDate.day());
        Datetime endDt   = startDt.addDays(1);
        
        List<UserLoginHistory__c> logs = [
            SELECT Id,
                   CreatedDate,
                   Status__c,
                   UserManagementTemp__r.Name,
                   UserManagementTemp__r.Account__c,
                   UserManagementTemp__r.Company__c
            FROM UserLoginHistory__c
            WHERE CreatedDate >= :startDt
              AND CreatedDate <  :endDt
            ORDER BY CreatedDate DESC
        ];

        // 3. 其餘條件在 Apex 迴圈裡過濾
        for (UserLoginHistory__c log : logs) {
            // 先把 "請選擇" 當成空值處理
            if (status == '請選擇')   status   = null;
            if (userTempId == '請選擇') userTempId = null;
            if (company == '請選擇')  company  = null;
            
            // 然後再做條件判斷
            if (String.isNotBlank(status) && log.Status__c != status) {
                continue;
            }
            
            if (String.isNotBlank(userTempId)) {
                if (log.UserManagementTemp__r == null || log.UserManagementTemp__r.Name != userTempId) {
                    continue;
                }
            }
            
            if (String.isNotBlank(company)) {
                if (log.UserManagementTemp__r == null || log.UserManagementTemp__r.Company__c != company) {
                    continue;
                }
            }
            
            // 4. 組 Wrapper 回前端
            LoginHistoryResultWrapper w = new LoginHistoryResultWrapper();
            w.Id                   = log.Id;
            w.UserManagementTempId = (log.UserManagementTemp__r != null) ? log.UserManagementTemp__r.Name      : null;
            w.Account              = (log.UserManagementTemp__r != null) ? log.UserManagementTemp__r.Account__c : null;
            w.Company              = (log.UserManagementTemp__r != null) ? log.UserManagementTemp__r.Company__c : null;
            w.Status               = log.Status__c;
            w.CreatedDate          = log.CreatedDate;
            w.CreatedDateGmtStr    = log.CreatedDate.formatGmt('yyyy-MM-dd HH:mm:ss');
            results.add(w);
        }

        
        return results;
    }


    
    private static String processHtmlContent(String htmlContent) {
        if (String.isBlank(htmlContent)) return '';
        
        String processedContent = htmlContent
            .replace('\\', '')
            .replace('\\"', '"')
            .replace('\\n', '\n')
            .replace('\\r', '\r');
        
        return processedContent;
    }
    
