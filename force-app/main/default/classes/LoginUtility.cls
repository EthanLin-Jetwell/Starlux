public without sharing class LoginUtility {
    
    // 驗證帳號及密碼
    public static Boolean verifyAccountLogin(UserManagement__c acc, String UserName, String password) {
        system.debug('User Account: ' + acc);
        system.debug('UserName: ' + acc.Account__c);
        system.debug('Password: ' + acc.Password__c);
        
        // 檢查 acc 是否存在且 email 和 password 匹配
        if (acc != null && acc.Account__c == UserName && acc.Password__c == password) {
            return true;  // 驗證通過
        }
        return false;  // 驗證失敗
    }
    
    // 檢查帳號是否過期
    public static Boolean isPasswordExpired(UserManagement__c acc) {
        if (acc.ExpireDate__c != null) {
            // 檢查密碼過期日期是否早於今天
            return acc.ExpireDate__c < System.today();
        }
        return false;  // 如果無過期日期，視為未過期
    }
    
    // 檢查帳號是否啟用
    public static Boolean isAccountActive(UserManagement__c acc) {
        return acc.Verify__c == true;
    }
    
    public static void saveLoginCookie(String accountId) {
        Pagereference currentPage = ApexPages.currentPage();
        Cookie accountIdCookie = new Cookie('accountId', accountId, null, 86400, false);//cookie存24小時
        currentPage.setCookies(new Cookie[]{accountIdCookie});
    }
    
