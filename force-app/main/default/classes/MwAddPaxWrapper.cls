        public String nameListNo {get;set;} 
        public String type {get;set;} 
        public String lastName {get;set;} 
        public String firstName {get;set;} 
        public String dateOfBirth {get;set;} 
        public String title {get;set;}
        public String gender {get;set;}
        // 2025/3/13 new added field Ethan
        public String passportIssueCountry {get;set;} 
        public String passportNo {get;set;} 
        public String passportCountry {get;set;} 
        public String passportExpiryDate {get;set;} 
        public String visaNo {get;set;} 
        public String visaCountry {get;set;}
        public String visaExpiryDate {get;set;}
    }
    
    public class SsrList {
        public String nameListNo {get;set;} 
        public String type {get;set;} 
        public List<String> ssr {get;set;} 
    }
    
    public class Contact {
        public String email {get;set;} 
    }
    public class tourGuide {
        public String email {get;set;}
        public String phone {get;set;}
    }
    
    public String pnr {get;set;} 
    public Contact contact {get;set;} 
    public List<Passengers> passengers {get;set;} 
    public List<Legs> legs {get;set;} 
    public tourGuide tourGuide {get;set;}
    
