public class MwAddPaxWrapper {
    
    public MwAddPaxWrapper(Case currCase, Seat_Allocation_and_Ticketing__c sat, String amadeusPost, List<Name_List__c> nameListList) {
        this.pnr = sat.Booking_Reference_PNR__c;
        this.contact = new contact();
        this.contact.Email = amadeusPost;
        this.passengers = new List<Passengers>();
        this.legs = new List<Legs>();
        this.tourguide=new tourGuide();
        tourguide.email=sat.Tour_Guide_Email__c;
        tourguide.phone=sat.Tour_Guide_Phone__c;
        for (Name_List__c nl : nameListList) {
            if(nl.Passenger_Type__c == 'adult' || nl.Passenger_Type__c == 'child') {
                Passengers currPassengers = new Passengers();
                currPassengers.type = nl.Passenger_Type__c;
                currPassengers.title = nl.Title__c;
                currPassengers.firstName = nl.First_Name__c;
                currPassengers.lastName = nl.Last_Name__c;
                if(nl.Date_of_Birth__c != null) {
                    currPassengers.dateOfBirth = String.valueOf(nl.Date_of_Birth__c).replace('-', '/');
                }
                currPassengers.nameListNo = nl.Name;
                currPassengers.gender = nl.Gender__c;
                
                // 2025/3/13 new added field Ethan
                if (
                    nl.Passport_Issue_Country__c != null &&
                    nl.Passport_No__c != null &&
                    nl.Passport_Country__c != null &&
                    nl.Passport_Expiry_Date__c != null
                ) {
                    currPassengers.passportIssueCountry = nl.Passport_Issue_Country__c;
                    currPassengers.passportNo = nl.Passport_No__c;
                    currPassengers.passportCountry = nl.Passport_Country__c;
                    currPassengers.passportExpiryDate = nl.Passport_Expiry_Date__c != null ? String.valueOf(nl.Passport_Expiry_Date__c).replace('-', '/') : null;
                }
                if (
                    nl.VISA_No__c != null &&
                    nl.VISA_Country__c != null &&
                    nl.VISA_Expiry_Date__c != null
                ) {
                    currPassengers.visaNo = nl.VISA_No__c;
                    currPassengers.visaCountry = nl.VISA_Country__c;
                    currPassengers.visaExpiryDate = nl.VISA_Expiry_Date__c != null ? String.valueOf(nl.VISA_Expiry_Date__c).replace('-', '/') : null;
                }
                
                this.passengers.add(currPassengers);
            }
            if(nl.INF_Passenger_Type__c == 'infant') {
                Passengers currPassengers = new Passengers();
                currPassengers.type = nl.INF_Passenger_Type__c;
                currPassengers.title = nl.INF_Title__c;
                currPassengers.firstName = nl.INF_First_Name__c;
                currPassengers.lastName = nl.INF_Last_Name__c;
                currPassengers.dateOfBirth = String.valueOf(nl.INF_Date_of_Birth__c).replace('-', '/');
                currPassengers.nameListNo = nl.Name;
                currPassengers.gender = nl.InfantGender__c;
                
                // 2025/3/13 new added field Ethan
                if (
                    nl.INF_Passport_Issue_Country__c != null &&
                    nl.INF_Passport_No__c != null &&
                    nl.INF_Passport_Country__c != null &&
                    nl.INF_Passport_Expiry_Date__c != null
                ) {
                    currPassengers.passportIssueCountry = nl.INF_Passport_Issue_Country__c;
                    currPassengers.passportNo = nl.INF_Passport_No__c;
                    currPassengers.passportCountry = nl.INF_Passport_Country__c;
                    currPassengers.passportExpiryDate = nl.INF_Passport_Expiry_Date__c != null ? String.valueOf(nl.INF_Passport_Expiry_Date__c).replace('-', '/'):null;
                }
                if (
                    nl.INF_VISA_No__c != null &&
                    nl.INF_VISA_Country__c != null &&
                    nl.INF_VISA_Expiry_Date__c != null
                ) {
                    currPassengers.visaNo = nl.INF_VISA_No__c;
                    currPassengers.visaCountry = nl.INF_VISA_Country__c;
                    currPassengers.visaExpiryDate = nl.INF_VISA_Expiry_Date__c != null ? String.valueOf(nl.INF_VISA_Expiry_Date__c).replace('-', '/'):null;
                }
                this.passengers.add(currPassengers);
            }
        }
        
        List<String> routeSplit = new List<String>();
        if (currCase.Route__c.contains('one-way')) {
            routeSplit.add(currCase.Route__c.substring(0, 3));
            routeSplit.add(currCase.Route__c.substring(4, 7));
        }
        else if(currCase.Route__c.contains(' X ')) {
            //make routeSplit size as 3 like normal round trip. by Chris Hsu 20220104
            routeSplit.add(currCase.Route__c.substring(0, 3));
            routeSplit.add(currCase.Route__c.substring(4, 7));
            routeSplit.add(currCase.Route__c.substring(10, 13));
        }
        else {
            routeSplit = currCase.Route__c.split('-');
        }
        
        for (Integer i = 1; i < routeSplit.size(); i++) {
            Legs currLegs = new Legs();
            currLegs.sorting = i;
            currLegs.ssrList = new List<SsrList>();
            for (Name_List__c nlOther : nameListList) {
                if(nlOther.Passenger_Type__c == 'adult' || nlOther.Passenger_Type__c == 'child') {
                    SsrList currSsrList = new SsrList();
                    currSsrList.ssr = new List<String>();
                    currSsrList.nameListNo = nlOther.Name;
                    currSsrList.type = nlOther.Passenger_Type__c;
                    if (nlOther.Meal_Selection_All_Sector__c != null) {
                        currSsrList.ssr.add(nlOther.Meal_Selection_All_Sector__c);
                    } else if (i == 1) {
                        if (sat.Departing_Flight_Number__c == 'none' && nlOther.Meal_Selection_Returning_Only__c != null) {
                            currSsrList.ssr.add(nlOther.Meal_Selection_Returning_Only__c);
                        } else if (sat.Departing_Flight_Number__c != 'none' && nlOther.Meal_Selection_Departing_Only__c != null) {
                            currSsrList.ssr.add(nlOther.Meal_Selection_Departing_Only__c);
                        }
                    } else if (i == 2) {
                        if (nlOther.Meal_Selection_Returning_Only__c != null) {
                            currSsrList.ssr.add(nlOther.Meal_Selection_Returning_Only__c);
                        }
                    }
                    if (i == 1) {
                        if (sat.Departing_Flight_Number__c == 'none' && nlOther.Special_Service_Returning__c != null) {
                            currSsrList.ssr.add(nlOther.Special_Service_Returning__c);
                        } else if (sat.Departing_Flight_Number__c != 'none' && nlOther.Special_Service_Departing__c != null) {
                            currSsrList.ssr.add(nlOther.Special_Service_Departing__c);
                        }
                    }
                    if (i == 2) {
                        if (nlOther.Special_Service_Returning__c != null) {
                            currSsrList.ssr.add(nlOther.Special_Service_Returning__c);
                        }
                    }
                    currLegs.ssrList.add(currSsrList);
                }
                if(nlOther.INF_Passenger_Type__c == 'infant') {
                    SsrList currSsrList = new SsrList();
                    currSsrList.ssr = new List<String>();
                    currSsrList.nameListNo = nlOther.Name;
                    currSsrList.type = nlOther.INF_Passenger_Type__c;
                    if (nlOther.INF_Meal_Selection_All_Sector__c != null) {
                        currSsrList.ssr.add(nlOther.INF_Meal_Selection_All_Sector__c);
                    } else if (i == 1) {
                        if (sat.Departing_Flight_Number__c == 'none' && nlOther.INF_Meal_Selection_Returning_Only__c != null) {
                            currSsrList.ssr.add(nlOther.INF_Meal_Selection_Returning_Only__c);
                        } else if (sat.Departing_Flight_Number__c != 'none' && nlOther.INF_Meal_Selection_Departing_Only__c != null) {
                            currSsrList.ssr.add(nlOther.INF_Meal_Selection_Departing_Only__c);
                        }
                    } else if (i == 2) {
                        if (nlOther.INF_Meal_Selection_Returning_Only__c != null) {
                            currSsrList.ssr.add(nlOther.INF_Meal_Selection_Returning_Only__c);
                        }
                    }
                    currLegs.ssrList.add(currSsrList);
                }
            }
            this.legs.add(currLegs);
        }
        
    }
    
    public class Legs {
        public Integer sorting {get;set;} 
        public List<SsrList> ssrList {get;set;} 
    }
    
