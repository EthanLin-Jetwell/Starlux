public class NavbarController {
    public String userName { get; private set; }
    public String userRole { get; private set; }
    public String userEmail { get; private set; }
    public String userENGName { get; private set; }
    public String userCompany { get; private set; }
    public String accountId { get;set; }
    


    public NavbarController() {
        accountId = CommonController.getAccountIdFromCookie();
        if (String.isBlank(accountId)) {
            // 處理未登入的情況,例如重定向到登入頁面
            ApexPages.addMessage(new ApexPages.Message(ApexPages.Severity.ERROR, '請先登入'));
            PageReference loginPage = Page.Login; // 假設您的登入頁面名為 Login
            loginPage.setRedirect(true);
            loginPage.getParameters().put('retUrl', ApexPages.currentPage().getUrl());
            // 注意: 這裡不會立即重定向,因為這是在構造函數中
            // 您需要在頁面的 action 屬性中調用一個方法來執行重定向
        } else {
            // 使用 accountId 進行相關操作
            init();
        }
    }

    private void init() {
        
        if (String.isNotBlank(accountId)) {
            try {
                UserManagement__c user = [SELECT Id, Email__c, CN_Passport__c, Account__c,Role__c,Company__c,Company__r.Name,EN_Passport__c
                                  FROM UserManagement__c 
                                  WHERE Id = :accountId 
                                  LIMIT 1];
                
                if (user != null) {
                    userRole = user.Role__c;
                    userName = user.CN_Passport__c;
                    userENGName = user.EN_Passport__c;
                    userCompany = user.Company__r.Name;
                    userEmail = user.Email__c;
                }
            } catch (Exception e) {
                // 處理查詢異常，例如記錄日誌
                System.debug('Error querying UserManagement__c: ' + e.getMessage());
            }
        }
    }
    
    public Pagereference logout() {
        return PageUtility.toPage(UrlConstants.LOGIN);
    }
    
