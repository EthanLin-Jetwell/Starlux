    // only TravellerInfo__c of the same flightnumber, pnr, operation, etc
    @future
    public static void log(String operation, String context, String flightNumber, Date flightDate, String origin, String pnr, Id userManagementId) {
        // Get the describe results for both picklist fields
        Schema.DescribeFieldResult operationFieldDescribe = OperationHistory__c.Operation__c.getDescribe();
        Schema.DescribeFieldResult contextFieldDescribe = OperationHistory__c.Menu__c.getDescribe();
        
        List<Schema.PicklistEntry> operationPicklistValues = operationFieldDescribe.getPicklistValues();
        List<Schema.PicklistEntry> contextPicklistValues = contextFieldDescribe.getPicklistValues();
        
        Boolean isValidOperation = false;
        Boolean isValidContext = false;
        
        operation = translateOperation(operation);
        context = translateMenu(context);
        
        for (Schema.PicklistEntry entry : operationPicklistValues) {
            if (entry.getValue() == operation) {
                isValidOperation = true;
                break;
            }
        }
        
        for (Schema.PicklistEntry entry : contextPicklistValues) {
            if (entry.getValue() == context) {
                isValidContext = true;
                break;
            }
        }
        
        if (!isValidOperation || !isValidContext) {
            throw new AuraHandledException('Invalid operation or context value. Must match picklist values.');
        }
        
        UserManagementTemp__c userTemp = [SELECT Id, CN_Passport__c FROM UserManagementTemp__c WHERE User_Management__c = :userManagementId LIMIT 1];
        
        // Use the fields you mentioned to find existing OperationHistory__c
        OperationHistory__c history;

        if (history == null) {
            history = new OperationHistory__c(
                FlightNumber__c = normalizeFlightNumber(flightNumber),
                DepartureDate__c = flightDate,
                Departure__c = origin,
                PNR__c = pnr,
                Operation__c = operation,
                Menu__c = context,
                UserManagement__c = userManagementId,
                User_Management_Temp__c = userTemp.Id
            );
            insert history;
        }
        
        // Now insert OperationHistoryDetail__c records
        List<OperationHistoryDetail__c> details = new List<OperationHistoryDetail__c>();
        //Field Label
        switch on operation {
            when '搜尋' {
                Map<String, Schema.SObjectField> fieldMap = Schema.getGlobalDescribe().get('OperationHistory__c').getDescribe().fields.getMap();
                
                String value = '';
                
                Map<String, Object> info = history.getPopulatedFieldsAsMap();
                for(String apiName : new String[]{'FlightNumber__c', 'DepartureDate__c', 'Departure__c', 'PNR__c'}) {
                    String label = fieldMap.get(apiName).getDescribe().getLabel();
                    value += '<p>'+label+' : '+String.valueOf((apiName == 'DepartureDate__c')? Date.valueOf(info.get(apiName)) : info.get(apiName))+'</p><p><span style="color: rgb(68, 68, 68); background-color: rgb(255, 255, 255);">\n';
                }
                
                OperationHistoryDetail__c detail = new OperationHistoryDetail__c(
                    OperationHistory__c = history.Id,
                    CustomerName__c = operation,
                    Value__c = value
                );
                
                details.add(detail);
            }
            when '輸出' {
                String value = '綜整表：' + pnr + ',Group Summary';
                OperationHistoryDetail__c detail = new OperationHistoryDetail__c(
                    OperationHistory__c = history.Id,
                    CustomerName__c = value,
                    Value__c = value
                );
                
                details.add(detail);
            }
        }

        if (!details.isEmpty()) {
            insert details;
        }
    }
    
