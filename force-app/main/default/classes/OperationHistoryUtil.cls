    public static void log(String operation, String context, String flightNumber, Date flightDate, String origin, String pnr, Id userManagementId, List<Id> digitalFileIds, List<Id> paperFileIds) {
        List<String> digitalFileNames = new List<String>();
        List<String> paperFileNames = new List<String>();
        
        for(TravellerInfo__c t : [SELECT LastName__c, FirstName__c, Title__c FROM TravellerInfo__c WHERE Id IN :digitalFileIds]) {
            digitalFileNames.add((t.LastName__c + '/' + t.FirstName__c +
                                  (String.isBlank(t.Title__c) ? '' : ' ' + t.Title__c)).toUpperCase());
        }
        
        for(TravellerInfo__c t : [SELECT LastName__c, FirstName__c, Title__c FROM TravellerInfo__c WHERE Id IN :paperFileIds]) {
            paperFileNames.add((t.LastName__c + '/' + t.FirstName__c +
                                  (String.isBlank(t.Title__c) ? '' : ' ' + t.Title__c)).toUpperCase());
        }
        
        // Get the describe results for both picklist fields
        Schema.DescribeFieldResult operationFieldDescribe = OperationHistory__c.Operation__c.getDescribe();
        Schema.DescribeFieldResult contextFieldDescribe = OperationHistory__c.Menu__c.getDescribe();
        
        List<Schema.PicklistEntry> operationPicklistValues = operationFieldDescribe.getPicklistValues();
        List<Schema.PicklistEntry> contextPicklistValues = contextFieldDescribe.getPicklistValues();
        
        Boolean isValidOperation = false;
        Boolean isValidContext = false;
        
        operation = translateOperation(operation);
        context = translateMenu(context);
        
        for (Schema.PicklistEntry entry : operationPicklistValues) {
            if (entry.getValue() == operation) {
                isValidOperation = true;
                break;
            }
        }
        
        for (Schema.PicklistEntry entry : contextPicklistValues) {
            if (entry.getValue() == context) {
                isValidContext = true;
                break;
            }
        }
        
        if (!isValidOperation || !isValidContext) {
            throw new AuraHandledException('Invalid operation or context value. Must match picklist values.');
        }
        
        if(digitalFileNames.isEmpty() && paperFileNames.isEmpty()) {
            return;
        }
        
        UserManagementTemp__c userTemp = [SELECT Id, CN_Passport__c FROM UserManagementTemp__c WHERE User_Management__c = :userManagementId LIMIT 1];
        
        // Use the fields you mentioned to find existing OperationHistory__c
        OperationHistory__c history;

        if (history == null) {
            history = new OperationHistory__c(
                FlightNumber__c = normalizeFlightNumber(flightNumber),
                DepartureDate__c = flightDate,
                Departure__c = origin,
                PNR__c = pnr,
                Operation__c = operation,
                Menu__c = context,
                UserManagement__c = userManagementId,
                User_Management_Temp__c = userTemp.Id
            );
            insert history;
        }
        
        // Now insert OperationHistoryDetail__c records
        List<OperationHistoryDetail__c> details = new List<OperationHistoryDetail__c>();
        
        if(operation == '輸出' || operation == '寄送')  {
            for(String filename : digitalFileNames) {
                String value = '電子登機證 : '+filename;
                OperationHistoryDetail__c detail = new OperationHistoryDetail__c(
                    OperationHistory__c = history.Id,
                    CustomerName__c = value,
                    Value__c = value
                );
                details.add(detail);
            }
            
            String title = '紙本登機證 : '+pnr+', GROUP Boarding Pass';
            String value = '';
            for(String fileName : paperFileNames) {
                value += fileName+'\n';
            }
            OperationHistoryDetail__c detail = new OperationHistoryDetail__c(
                OperationHistory__c = history.Id,
                CustomerName__c = title,
                Value__c = value
            );
            details.add(detail);
        }
        
        if (!details.isEmpty()) {
            insert details;
        }
    }
    
    public static String translateOperation(String operation) {
        Map<String, String> operationMap = new Map<String, String>{
            'create' => '新增',
            'delete' => '刪除',
            'update' => '修改',
            'send'   => '寄送',
            'export' => '輸出',
            'search' => '搜尋'
        };

        String lowerAction = operation != null ? operation.toLowerCase().trim() : '';
        return operationMap.containsKey(lowerAction) ? operationMap.get(lowerAction) : operation;
    }
    
    public static String translateMenu(String menu) {
        Map<String, String> menuMap = new Map<String, String>{
            'EditTraveler'       => '編輯旅客旅行文件資料',
            'SeatSelection'      => '團體選位/換位',
            'CheckIN'            => '團體報到',
            'BoardingPassPrinting' => '登機證列印/寄送',
            'CheckInReport'      => '團體報到結果綜整'
        };

        String normalized = menu != null ? menu.trim() : '';
        return menuMap.containsKey(normalized) ? menuMap.get(normalized) : menu;
    }
    
    public static String normalizeFlightNumber(String flightNumber) {
        if (String.isBlank(flightNumber)) {
            return null;
        }
        
        // Remove any spaces
        String cleaned = flightNumber.trim();
        
        // Match: letters, optional dash, digits
        Pattern p = Pattern.compile('^([A-Za-z]+)-?(\\d+)$');
        Matcher m = p.matcher(cleaned);
        
        if (m.matches()) {
            String prefix = m.group(1);
            String num = m.group(2);
            
            // Pad number with leading zeros to 4 digits
            while (num.length() < 4) {
                num = '0' + num;
            }
            
            return prefix + '-' + num;
        }
        
        // If it doesn't match the expected pattern, return original
        return flightNumber;
    }
    
