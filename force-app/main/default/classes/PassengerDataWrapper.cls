    public static List<TravellerInfo__c> parsePassengerInfo(List<PassengerInfo> passengerData) {
        List<TravellerInfo__c> travellers = new List<TravellerInfo__c>();

        for (PassengerInfo pInfo : passengerData) {
            TravellerInfo__c t = new TravellerInfo__c();
            
            // Basic fields
            t.Id = pInfo.recordId;
            t.FlightNumber__c = pInfo.FlightNumber;
            t.DepartureLocation__c = pInfo.DepartureLocation;
            t.Destination__c = pInfo.Destination;
            t.DepartureDate__c = pInfo.DepartureDate;
            t.DepartureDate2__c = pInfo.DepartureDate2;
            t.LastName__c = pInfo.lastName;
            t.FirstName__c = pInfo.firstName;
            t.title__c = pInfo.title;
            t.gender__c = pInfo.gender;
            t.country__c = pInfo.nationality;
            t.DateOfBirth__c = convertToCompactFormat(pInfo.birthDate);
            t.PassengerType__c = pInfo.passengerType;
            t.US_Bound__c = pInfo.usBound;
            t.PassengerType__c = pInfo.PassengerType;
            t.PT__c = pInfo.pt;
            t.ST01__c = pInfo.st;
            t.ST02__c = pInfo.st2;
            t.Seat__c = pInfo.seat;
            t.RQST_OT__c = pInfo.RQST_OT;
            t.APE__c = pInfo.APE;
            t.APM__c = pInfo.APM;
			t.IATA_MSO_Code__c = pInfo.IATA;
            t.CheckDisable__c = pInfo.CheckDisable;
            //t.C3Status__c = pInfo.C3Status;
            t.C3ExternalFinished__c = pInfo.C3ExternalFinished;
            t.C4BoardingPassPrinted__c = pInfo.C4BoardingPassPrinted;
            t.C4BoardingPassPrintedTime__c = pInfo.C4BoardingPassPrintedTime;
            //t.C4BoardingPassPrintedTimeGMTLocal__c = pInfo.C4BoardingPassPrintedTimeGMTLocal;
            t.C4EBoardingPass__c = pInfo.C4EBoardingPass;
            t.C4PaperBoardingPass__c = pInfo.C4PaperBoardingPass;
            t.PrintDisable__c = pInfo.PrintDisable;
            t.PAX__c = pInfo.PAX;
            t.UCI__c = pInfo.UCI;
            t.DID__c = pInfo.DID;
            t.CAS__c = pInfo.CAS;
            t.Service__c = pInfo.Service;
            t.Meal__c = pInfo.Meal;
            t.C1Finished__c = pInfo.C1Finished?? false;
            t.C4Finished__c = pInfo.C4Finished?? false;
            
                        
            
            for (DocumentInfo doc : pInfo.documents) {
                // Passport fields
                if (doc.docType == 'Ë≠∑ÁÖß') {
                    t.PassportNumber__c = doc.docNumber;
                    t.PassportExpiryDate__c = convertToCompactFormat(doc.expiryDate);
                    t.PassportIssuingCountry__c = doc.issuingCountry;
                    t.USAStreet__c = doc.USStreet;
                    t.USACity__c = doc.UScity;
                    t.USAState__c = doc.USstate;
                    t.USAZipCode__c = doc.USpostalcode;
                    t.USAEmergencyContactName__c = doc.UScontact;
                    t.USAEmergencyContactNationality__c = doc.UScontactNationality;
                    t.USAEmergencyContactPhone__c = doc.UScontactphone;
                }
                
                // Visa fields
                if (doc.docType == 'Á∞ΩË≠â') {
                    t.VisaNumber__c = doc.visaNumber;
                    t.VisaExpiryDate__c = convertToCompactFormat(doc.visaExpiryDate);
                    t.VisaIssuingCountry__c = doc.visaIssuingCountry;
                }
            }
            travellers.add(t);
        }
        
        return travellers;
    }
    
    
