    // 01JAN90 ‚Üí 1990-01-01
    // to convert 
    public static String convertToIsoDate(String input) {
        if (String.isBlank(input) || input.length() != 7) return null;
        
        try {
            String day = input.substring(0, 2);
            String monthStr = input.substring(2, 5).toUpperCase();
            String yearStr = input.substring(5, 7);
            
            Map<String, String> monthMap = new Map<String, String>{
                'JAN' => '01', 'FEB' => '02', 'MAR' => '03',
                    'APR' => '04', 'MAY' => '05', 'JUN' => '06',
                    'JUL' => '07', 'AUG' => '08', 'SEP' => '09',
                    'OCT' => '10', 'NOV' => '11', 'DEC' => '12'
                    };
                        
                        if (!monthMap.containsKey(monthStr)) return null;
            
            Integer year = Integer.valueOf(yearStr);
            Integer cutoff = Math.mod(System.today().year(), 100);
            String fullYear = (year > cutoff) ? '19' + yearStr : '20' + yearStr;
            
            return fullYear + '-' + monthMap.get(monthStr) + '-' + day;
        } catch (Exception e) {
            return null;
        }
    }
	// for Expirydate
    // 01JAN90 ‚Üí 1990-01-01
    // to convert 
    public static String convertToIsoDateExpiry(String input) {
        if (String.isBlank(input) || input.length() != 7) return null;
        
        try {
            String day = input.substring(0, 2);
            String monthStr = input.substring(2, 5).toUpperCase();
            String yearStr = input.substring(5, 7);
            
            Map<String, String> monthMap = new Map<String, String>{
                'JAN' => '01', 'FEB' => '02', 'MAR' => '03',
                    'APR' => '04', 'MAY' => '05', 'JUN' => '06',
                    'JUL' => '07', 'AUG' => '08', 'SEP' => '09',
                    'OCT' => '10', 'NOV' => '11', 'DEC' => '12'
                    };
                        
            if (!monthMap.containsKey(monthStr)) return null;

            Integer yy = Integer.valueOf(yearStr);
            Integer currentYear = System.today().year();
            Integer currentCentury = currentYear / 100;
            Integer currentYY = Math.mod(System.today().year(), 100);
    
            Integer fullYear = (yy >= currentYY)
                ? (currentCentury * 100 + yy)
                : ((currentCentury + 1) * 100 + yy);

            return String.valueOf(fullYear) + '-' + monthMap.get(monthStr) + '-' + day;
        } catch (Exception e) {
            return null;
        }
    }
    
    //1990-01-01 ‚Üí 01JAN90
    // to convert 
    public static String convertToCompactFormat(String isoDate) {
        if (String.isBlank(isoDate)) return null;
        
        try {
            Date d = Date.valueOf(isoDate);  // Ê†ºÂºèÂøÖÈ†àÊòØ yyyy-MM-dd
            
            String day = String.valueOf(d.day());
            if (day.length() == 1) day = '0' + day;
            
            List<String> monthNames = new List<String>{
                'JAN', 'FEB', 'MAR', 'APR', 'MAY', 'JUN',
                    'JUL', 'AUG', 'SEP', 'OCT', 'NOV', 'DEC'
                    };
                        String month = monthNames[d.month() - 1];
            
            String year = String.valueOf(d.year()).right(2);
            
            return day + month + year;
        } catch (Exception e) {
            return null;
        }
    }
    
