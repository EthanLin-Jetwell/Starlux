    public ResetPasswordController() {
        emailLogId = ApexPages.currentPage().getParameters().get('emailLogId');
        isExpired = false;
        isVisible = false;
        
        if (String.isNotBlank(emailLogId)) {
			//從信箱來
            EmailLog__c emailLog = [SELECT Id, User__c, Expire__c FROM EmailLog__c WHERE Id = :emailLogId];
            staffId = emailLog?.User__c;
            isExpired = emailLog.Expire__c;
        } else {
            //密碼逾期，自動跳轉
            staffId = ApexPages.currentPage().getParameters().get('accountId');
        }
    }
	
    
    
    
    
    public Pagereference resetPassword() {
        // 重置狀態
        isSamePassword = false;
        isResetPassword = false;
        isDifferentPassword = false;
    
        system.debug(staffId);
        staff = [SELECT Password__c,Password2__c,Password3__c FROM UserManagement__c WHERE id = :staffId LIMIT 1];

        //UserManagement__c staff = UserUtilities.getUserById(staffId);
        system.debug(staff);
        if (staff == null) {
            return null;
        }
        // 驗證是否與舊密碼相同 
        if (password == staff.PassWord__c||password == staff.PassWord2__c||password == staff.PassWord3__c) {
            isDifferentPassword = true; 
            return null; 
        }
    
        system.debug(staff.PassWord__c);
    
        // 查詢 B2B_Parameter__c 的 Value__c 作為過期天數
        Integer daysToAdd = 90; // 預設值
        try {
            B2B_Parameter__c parameter = [SELECT Value__c FROM B2B_Parameter__c WHERE Name = 'P003' LIMIT 1];
            if (parameter != null && String.isNotBlank(parameter.Value__c)) {
                daysToAdd = Integer.valueOf(parameter.Value__c);
            }
        } catch (Exception e) {
            system.debug('無法取得參數 P003：' + e.getMessage());
        }
    
        // 發送驗證信，返回登入頁
        isResetPassword = true;
        staff.PassWord__c = newpassword;
        Date today = Date.today();
        staff.ExpireDate__c = today.addDays(daysToAdd); // 使用查詢到的天數
        staff.Verify__c = true;
        update staff;
        return null;
    }

