@RestResource(urlMapping='/RestMiddlewareApi/*')
global class RestMiddlewareApi {
    global class CreditBalanceException extends Exception {}
    
    @HttpPost
    global static ResponseModel doPost(String identify, String requisitionID, String serialID, String diagramID, String applicantID, String applicantName, String applicantDept,
                             String applicantDeptName, String applicantDateTime, String TA_Name, String TA_BusinessNumber, String TA_Address, String TA_Phone, 
                             String TA_PIC, String TA_PIC_Phone, String PIC_Email, String FunctionType, String Remark, String BG_VaildDate, String BG_ExpiryDate, 
                             String BG_Category, String Bank, String BG_Number, String Money, String CityCode, String CreditAmount, String WithdrawBeforeExpiry, 
                             String SF_BG_No, String AccountUnlock)
    {
        Integer httpStatusCode = 403;
        String resultMessage = 'fail';
        String recordId;
        String SF_Return_BG_No;
        
        //Apply for Credit Limit
        try{
            if(FunctionType == 'A') {
                Bank_Guarantee__c bankGuarantee = new Bank_Guarantee__c ();
                //bankGuarantee.AccountId__c = [SELECT Id FROM Account WHERE Name LIKE : TA_Name LIMIT 1].Id; 20231115 Aaron
                bankGuarantee.AccountId__c = [SELECT Id FROM Account WHERE Name LIKE : TA_Name AND Business_Number__c = : TA_BusinessNumber LIMIT 1].Id; //20231115 Aaron
                //bankGuarantee.Business_Number__c = TA_BusinessNumber;
                bankGuarantee.Credit_Contact__c = TA_PIC;
                bankGuarantee.Credit_Contact_Email__c = PIC_Email;
                bankGuarantee.Credit_Contact_Phone__c = TA_PIC_Phone;
                bankGuarantee.Bank_Guarantee_Valid_Date__c = RestMiddlewareApi.parseDate(BG_VaildDate);
                bankGuarantee.Bank_Guarantee_Expiry_Date__c = RestMiddlewareApi.parseDate(BG_ExpiryDate);
                bankGuarantee.Bank_Guarantee_Category__c = BG_Category;
                bankGuarantee.Issuing_Bank__c = Bank;
                bankGuarantee.Certification_No__c = BG_Number;
                bankGuarantee.Currency__c = Money;
                bankGuarantee.City_Code__c = CityCode;
                bankGuarantee.Credit_Amount__c = Decimal.valueOf(CreditAmount);
                bankGuarantee.BPM_No__c = serialID;
                bankGuarantee.Applicant_Name__c = applicantName;
                bankGuarantee.Applicant_Department__c = applicantDeptName;
                bankGuarantee.Applicant_Number__c = applicantID;
                //bankGuarantee.Active__c = true; 
                bankGuarantee.Remark__c = Remark;
                Insert bankGuarantee;
                //Created
                httpStatusCode = 201;
                resultMessage = 'success';
                recordId = bankGuarantee.Id;
                SF_Return_BG_No = [SELECT Name FROM Bank_Guarantee__c WHERE Id =: bankGuarantee.Id].Name;
            } 
            //Withdraw Credit Limit
            else if(FunctionType == 'W') {
                Bank_Guarantee__c bankGuarantee = [//SELECT Id, Bank_Guarantee_Withdrawal_Date__c, Withdrawal_BPM_No__c, Applicant_Name__c, 
                                                   //Applicant_Department__c, Withdraw_Before_Expiry__c, Applicant_Number__c, Withdrawal_Remark__c 
                                                   SELECT Id, Bank_Guarantee_Expiry_Date__c, Bank_Guarantee_Withdrawal_Date__c, Withdrawal_BPM_No__c, Withdrawal_Applicant_Name__c, //20231116 Eli
                                                   Withdrawal_Applicant_Department__c, Withdraw_Before_Expiry__c, Withdrawal_Applicant_Number__c, Withdrawal_Remark__c, //20231116 Eli
                                                   AccountId__r.Credit_Balance__c, Credit_Amount__c, AccountId__r.Credit_Balance2__c, Active__c
                                                   FROM Bank_Guarantee__c 
                                                   // WHERE Certification_No__c = : BG_Number AND Business_Number__c = : TA_BusinessNumber LIMIT 1]; 20231115 Aaron
                                                   WHERE Name =:SF_BG_No AND Business_Number__c =: TA_BusinessNumber LIMIT 1]; //20231115 Aaron
                if(bankGuarantee.AccountId__r.Credit_Balance2__c - bankGuarantee.Credit_Amount__c < 0 && bankGuarantee.Active__c == true) { // Check Credit Balance // Eli add && bankGuarantee.Active__c == true on 20250820
                    throw new CreditBalanceException();
                } else {
                    //bankGuarantee.Bank_Guarantee_Withdrawal_Date__c = RestMiddlewareApi.parseDate(BG_ExpiryDate); 20231115 Aaron
                    bankGuarantee.Bank_Guarantee_Withdrawal_Date__c = RestMiddlewareApi.parseDate(applicantDateTime); //20231116 Eli
                    bankGuarantee.Withdrawal_BPM_No__c = serialID;
                    //bankGuarantee.Applicant_Name__c = applicantName;
                    //bankGuarantee.Applicant_Department__c = applicantDeptName;
                    bankGuarantee.Withdrawal_Applicant_Name__c = applicantName; //20231116 Eli
                    bankGuarantee.Withdrawal_Applicant_Department__c = applicantDeptName; //20231116 Eli
                    bankGuarantee.Withdraw_Before_Expiry__c = (WithdrawBeforeExpiry=='Yes')? true : false;
                    IF (WithdrawBeforeExpiry=='Yes') {bankGuarantee.Bank_Guarantee_Expiry_Date__c = RestMiddlewareApi.parseDate(BG_ExpiryDate);} //20231115 Eli
                    //bankGuarantee.Applicant_Number__c = applicantID;
                    bankGuarantee.Withdrawal_Applicant_Number__c = applicantID; //20231116 Eli
                    bankGuarantee.Withdrawal_Remark__c = Remark;
                    Upsert bankGuarantee;
                    //Created
                    httpStatusCode = 200;
                    resultMessage = 'success';
                    recordId = bankGuarantee.Id;
                    SF_Return_BG_No = [SELECT Name FROM Bank_Guarantee__c WHERE Id =: bankGuarantee.Id].Name;
                }
            }
            //Replenishment
            else if(FunctionType == 'R') {
                Replenishment__c replenishment = new Replenishment__c();
                replenishment.AccountId__c = [SELECT Id FROM Account WHERE Name = : TA_Name AND Business_Number__c = : TA_BusinessNumber LIMIT 1].Id;
                replenishment.Credit_Contact__c = TA_PIC;
                replenishment.Credit_Contact_Email__c = PIC_Email;
                replenishment.Credit_Contact_Phone__c = TA_PIC_Phone;
                replenishment.Currency__c = Money;
                replenishment.City_Code__c = CityCode;
                replenishment.Replenishment_Amount__c = Decimal.valueOf(CreditAmount);
                replenishment.BPM_No__c = serialID;
                replenishment.Applicant_Name__c = applicantName;
                replenishment.Applicant_Department__c = applicantDeptName;
                replenishment.Applicant_Number__c = applicantID;
                replenishment.Account_Unlocked__c= (AccountUnlock=='Yes')? 'Yes' : 'No';
                replenishment.Remark__c = Remark;
                Insert replenishment;
                //Created
                httpStatusCode = 201;
                resultMessage = 'success';
                recordId = replenishment.Id;
                SF_Return_BG_No = [SELECT Name FROM Replenishment__c WHERE Id =: replenishment.Id].Name;
            }
        } catch(CreditBalanceException ce) {
            //Bad request
            httpStatusCode = 400;
            resultMessage = 'Error: Create Balance < 0';
            return new ResponseModel(httpStatusCode, NULL, NULL,  NULL, resultMessage);         
        } catch(exception e) {
            //Bad request
            httpStatusCode = 400;
            resultMessage = 'Error: ' + e + 'at line: ' + e.getLineNumber();
            return new ResponseModel(httpStatusCode, NULL, NULL,  NULL, resultMessage);         
        }
        
        return new ResponseModel(httpStatusCode, NULL, recordId, SF_Return_BG_No, resultMessage);
    }
    
