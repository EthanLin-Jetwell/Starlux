public with sharing class SObjectUtil {
    
    public static SObject updateFirstObjectWithNonNullValueOfSecondObject(SObject object1, SObject object2) {
        for (SObjectField field : object1.getSObjectType().getDescribe().fields.getMap().values()) {
            try {
                Object opp2Field = object2.get(field);
                if (opp2Field != null) {
                    object1.put(field, opp2Field);
                }
            } catch (SObjectException ex) {
            }
        }
        return object1;
    }

    public static void printSObjectFieldAndValue(SObject objectToPrint) {
        System.debug('[4i] Printing SObject fields and values');
        for (SObjectField field : objectToPrint.getSObjectType().getDescribe().fields.getMap().values()) {
            try {
                Object fieldValue = objectToPrint.get(field);
                System.debug('[4i]' + field + '=>' + fieldValue);
            } catch (SObjectException ex) {
            }
        }
    }

