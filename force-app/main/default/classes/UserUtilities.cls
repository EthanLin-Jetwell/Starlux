public class UserUtilities {
    
	// 根據帳號、統一編號取得員工資料 For 登入
    public static UserManagement__c getUserByAccountAndIATAcode(String Account, String IATAcode) {
        List<UserManagement__c> UserList = [SELECT Id, Email__c, Password__c, Password2__c, Password3__c, Name, Verify__c,ExpireDate__c,IATA_MSO_Code__c,Account__c,Resend__c,UserManagementTemp__c 
                                          FROM UserManagement__c 
                                          WHERE Account__c = :Account AND IATA_MSO_Code__c = :IATAcode];
        if (!UserList.isEmpty()) {
            return UserList.get(0);
        }
        return null;
    }
    
    // 根據帳號、統一編號、電子郵件取得員工資料 For 忘記密碼
    public static UserManagement__c getUserByAccountAndIATAcode(String Account, String IATAcode, String email) {
        List<UserManagement__c> UserList = [SELECT Id, Email__c, Password__c, Password2__c, Password3__c, Name, Verify__c,ExpireDate__c,IATA_MSO_Code__c,Account__c,Resend__c,UserManagementTemp__c 
                                          FROM UserManagement__c 
                                          WHERE Account__c = :Account AND IATA_MSO_Code__c = :IATAcode AND Email__c =: email];
        if (!UserList.isEmpty()) {
            return UserList.get(0);
        }
        return null;
    }
    
    
    // 根據帳號取得員工資料
    public static UserManagement__c getUserByAccount(String UserName) {
        List<UserManagement__c> UserList = [SELECT Id, Email__c, Password__c, Password2__c, Password3__c, Name, Account__c,Resend__c,UserManagementTemp__c 
                                          FROM UserManagement__c WHERE Account__c = :UserName];
        if (!UserList.isEmpty()) {
            return UserList.get(0);
        }
        return null;
    }
    // 根據id取得員工資料
    public static UserManagement__c getUserById(String Id) {
        List<UserManagement__c> UserList = [SELECT Id, Email__c, Password__c, Password2__c, Password3__c, Name, Account__c,Resend__c,UserManagementTemp__c 
                                          FROM UserManagement__c WHERE Id = :Id];
        if (!UserList.isEmpty()) {
            return UserList.get(0);
        }
        return null;
    }
